            Errors += "Customer Details : Customer Code should not be empty<br/>";
            showErrorSnackbar("Customer Code should not be empty");
        }
        if ($("#txtName").val() == "") {
            IsValid = false;
            $("#divErrorOnName").html("Customer Name should not be empty");
            Errors += "Customer Details : Customer Name should not be empty<br/>";
            showErrorSnackbar("Customer Name should not be empty");
        }
        if ($("#cmdPriceType").val() == 0) {
            IsValid = false;
            $("#divErrorOnPriceType").html("Select Price Type");
            Errors += "Customer Details : Select Price Type<br/>";
            showErrorSnackbar("Select Price Type");
        }
        if ($("#cmbTaxType").val() == 0) {
            IsValid = false;
            $("#divErrorOnTaxType").html("Select Tax Type");
            showErrorSnackbar("Select Tax Type");
        }
        if ($("#txtPincode").val().length != 6) {
            IsValid = false;
            $("#divErrorOnPincode").html("Invalid Pincode format(Enter 6 numeric)");
            showErrorSnackbar("Invalid Pincode format(Enter 6 numeric)");
        }
        if ($("#cmbPayMode").val() == 7) {
            if ($("#cmbCreditTerm").val() == 0) {
                IsValid = false;
                $("#divErrorOnCreditTerm").html("Select Credit Date");
                showErrorSnackbar("Select Credit Date");
            }
        }
        ChkFormation = PhonenoValidation($("#txtPh1"), $("#divErrorOnPh1"), false);
        if (!ChkFormation) {
            IsValid = false;
        }
        ChkFormation = PhonenoValidation($("#txtPh2"), $("#divErrorOnPh2"), false);
        if (!ChkFormation) {
            IsValid = false;
        }
        ChkFormation = MobilenoValidation($("#txtMob1"), $("#divErrorOnMob1"), false);
        if (!ChkFormation) {
            IsValid = false;
        }
        ChkFormation = MobilenoValidation($("#txtMob2"), $("#divErrorOnMob2"), false);
        if (!ChkFormation) {
            IsValid = false;
        }
        ChkFormation = EmailValidation($("#txtEMail"), $("#divErrorOnEMail"), false);
        if (!ChkFormation) {
            IsValid = false;
        }
        ChkFormation = PANValidation($("#txtPAN"), $("#divErrorOnPAN"), false);
        if (!ChkFormation) {
            IsValid = false;
            showErrorSnackbar("Invalid PAN Format");
        }
        if ($("#txtAadhaar").val() != "") {
            ChkFormation = AadharValidation($("#txtAadhaar"), $("#divErrorOnAadhaar"), false);
            if (!ChkFormation) {
                IsValid = false;
                showErrorSnackbar("Invalid Aadhaar Format");
            }
        }
        ChkFormation = FSSAIValidation($("#txtFSSAI"), $("#divErrorOnFSSAI"), false);
        if (!ChkFormation) {
            IsValid = false;
            showErrorSnackbar("Invalid FSSAI Format");
        }
        ChkFormation = GSTINValidation($("#txtGSTIN"), $("#divErrorOnGSTIN"), false);
        if (!ChkFormation) {
            IsValid = false;
            showErrorSnackbar("Invalid GSTIN Format");
        }
        ChkFormation = ValidateBeatsalesman();
        if (!ChkFormation) {
            IsValid = false;
        }

        if (IsValid) {
            if (cfFocus != 3) { ConfirmDialogMsg("Do you want to save data?"); } else { SaveData(); }
        }
        else {
            //$("#dvErrorDetail").show();
            //$("#lblSaveError").html(Errors);
        }
    });
    function SaveData() {
        $("#btnSave").prop("disabled", true);
        AddRemarks();
        AddBeatSalesman();
        $(".clrdiv").html("");
        var name = $("#txtName").val();
        var code = $("#txtCode").val();
        var lstMaster = {
            "Mode": nMode,
            "ID": $("#txtID").val(),
            "Code": $("#txtCode").val(),
            "Name": $("#txtName").val(),
            "CustomerType": $("#cmbCustomerType").val(),
            "Ratings": $("#cmbRating").val(),
            "Billadd1": $("#txtBAdd1").val(),
            "Billadd2": $("#txtBAdd2").val(),
            "Billadd3": $("#txtBAdd3").val(),
            "Shipadd1": $("#txtSAdd1").val(),
            "shipadd2": $("#txtSAdd2").val(),
            "Shipadd3": $("#txtSAdd3").val(),
            "Pincode": $("#txtPincode").val(),
            "ContactPerson": $("#txtContactperson").val(),
            "Ph1": $("#txtPh1").val(),
            "Ph2": $("#txtPh2").val(),
            "Mob1": $("#txtMob1").val(),
            "Mob2": $("#txtMob2").val(),
            "Email": $("#txtEMail").val(),
            "PANNumber": $("#txtPAN").val(),
            "AadharNo": $("#txtAadhaar").val(),
            "DLNo20": $("#txtDL20").val(),
            "DLNo21": $("#txtDL21").val(),
            "FSSAINo": $("#txtFSSAI").val(),
            "StateID": $("#cmbState").val(),
            "GSTIN": $("#txtGSTIN").val(),
            "CreditTermID": $("#cmbCreditTerm").val(),
            "PaymentModeID": $("#cmbPayMode").val(),
            "TaxTypeID": $("#cmbTaxType").val(),
            //"OverDueValue": $("#txtID").val(),
            "OverDueInvCount": $("#txtODInvCount").val(),
            "CreditLimitValue": document.getElementById("txtCrLimitValue").getAttribute("txttag"),//$("#txtCrLimitValue").val(),
            "CreditLimitCount": $("#txtCrLimitInvCount").val(),
            "CreditlimitOS": document.getElementById("txtOSLimitValue").getAttribute("txttag"),//$("#txtOSLimitValue").val(),
            "PriceTypeID": $("#cmdPriceType").val(),
            "OwnerName": $("#txtOwnerName").val(),
            "DiscountPern": document.getElementById("txtDiscPern").getAttribute("txttag"),//$("#txtDiscPern").val(),
            "TrackPoint": $("#chkShowTP").is(":checked") ? 1 : 0,
            //"ClosingTrackPoint": $("#txtID").val(),
            "TCSTax": $("#chkApplyTCS").is(":checked") ? 1 : 0,
            "Latitude": $("#txtLocation").val(),
            "Distance": $("#txtDistance").val(),
            "Remark": $("#txtRemarks").val(),
            "Active": $("#chkActive").is(":checked") ? 1 : 0,
            "lstCustRemark": objcustRem,
            "UserID": UID,
            "BSM": objItemDetails
        };
            $.ajax({
                url: Aurl + "customermaster/save",
                type: 'POST',
                contentType: 'application/json; charset=utf-8',
                data: JSON.stringify(lstMaster),
                cache: false,
                processData: false,
                xhrFields: {
                    withCredentials: true
                },
                success: function (data) {
                    $("#btnSave").prop("disabled", false);
                    $.each(data, function (i, items) {
                        if (items.MsgID == 0) {
                            showSuccessSnackbar(items.Message);
                            ClearForm();
                            AllCustData();
                            GetData(4);
                            modal.style.display = "none";
                            var depfrom = $("#txtDependFrom").val();
                            if (window.opener && !window.opener.closed) {
                                if (depfrom == "0") {// invoice
                                    window.opener.GetData(3, code);
                                    //window.opener.$('#txtHProds').val(name)
                                }
                                window.close();
                            }

                        }
                        else if (items.Message == "Code") {
                            $("#divErrorOnCode").html("Code Already Exists");
                            showErrorSnackbar("Code Already Exists");
                        }
                        else if (items.Message == "Name") {
                            $("#divErrorOnName").html("Name Already Exists");
                            showErrorSnackbar("Name Already Exists");
                        }
                        else if (items.Message == "AccountName") {
                            $("#divErrorOnName").html("Account Name Already Exists in another master");
                            showErrorSnackbar("Account Name Already Exists in another master");
                        }
                        else if (items.Message == "OSPending") {
                            //$("#divErrorOnName").html("Outstanding Amount Pending. Cannot Deactivate");
                            showErrorSnackbar("Outstanding Amount Pending. Cannot Deactivate");
                        }

                        else {
                            //$("#divErrorOnCode").html(items.Message);
                            showErrorSnackbar(items.Message);
                        }
                    });
                }, error: function (data) {
                    $("#btnSave").prop("disabled", false);
                    if (data.status == "401") {
                        var msg = data.responseJSON.Message;
                        //showErrorSnackbar(msg + " Logout and Login again.");
                    }
                }
            });
    }
