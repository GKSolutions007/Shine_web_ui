    function ValidateBeatsalesman() {
        var IsValid = true;
        //BeatMandatoryinCustomer
        //$('#tblBeatSalesman
        var bearmaprows = TableBS.rows().count();
        
        if (bearmaprows > 0) {
            $('#tblBeatSalesman tr').each(function (i, items) {
                $(this).find("input").each(function (n) {
                    $(this).css("border-color", "black");
                    const fieldName = $(this).attr("name");
                    if (fieldName == "Beat") {
                        //const exists = arrBeat.indexOf($(this).val().trim()) !== -1;
                        var btarr = arrBeat.filter(a => a.label == $(this).val().trim());
                        if (btarr.length == 0) {
                            IsValid = false;
                            showErrorSnackbar("Select Beat Name from list");
                            $(this).css("border-color", "red");
                        }
                    }
                    if (fieldName == "Salesman") {
                        var smarr = arrSalesman.filter(a => a.label == $(this).val().trim());
                        if (smarr.length == 0) {
                            IsValid = false;
                            showErrorSnackbar("Select Salesman Name from list");
                            $(this).css("border-color", "red");
                        }
                    }
                });
            });
        }
        else {
            if (BeatMandatoryinCustomer == 1) {
                IsValid = false;
                showErrorSnackbar("Beat Mapping is mandatory.");
            }
        }
        return IsValid;
    }
