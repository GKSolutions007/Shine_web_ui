    function CheckStatutoryExists(strFormName, strFieldName, strValue) {
        var StrID = $("#txtID").val() == "" || $("#txtID").val() == null ? 0 : $("#txtID").val();
        $.ajax({
            url: Aurl + "customermaster/validatestatutory",
            type: 'get',
            contentType: 'json',
            data: { FormName: strFormName, FieldName: strFieldName, Value: strValue, ID: StrID },
            xhrFields: {
                withCredentials: true
            },
            success: function (data) {
                var Response = eval(data);
                if (Response) {
                    var Infodata = '';
                    $.each(Response, function (i, items) {
                        if (strFieldName == "GSTIN") {
                            Infodata += "&bull; " +items.Name + ", <br/>";
                        }
                    });
                    if (Infodata != '') {
                        if (strFieldName == "GSTIN") {
                            $("#divErrorOnGSTIN").html("GSTIN Already Used in This Customer(s)<br/>" + Infodata);
                        }
                    }
                }
            }
        });
    }
