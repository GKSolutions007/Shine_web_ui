            var url = $("#txtChildBankAccURL").val();
            if (url != "") {
                var id = event.target.id;
                if (id == "cmbAccNo") {
                    $("#lbliframeheading").text("Bank");
                    document.getElementById("ifrSMG").src = url + "&ParFFID=cmbAccNo";
                    // Prevent default action if any (e.g., browser F8 behavior)
                    event.preventDefault();
                    // Open the modal when F8 is pressed
                    modaliframe.style.display = "block";
                }
            }
