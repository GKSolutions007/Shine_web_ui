    function getProductBatchInfo(nProdID, nBranchID) {
        var IsValid = true;
        TableBatch.draw().clear();
        TableBatch.draw().clear();
        $(".lblBatchvalue").text("");
        if (nProdID == "") {
            IsValid = false;
            showErrorSnackbar("Select valid product");
        }
        if (nBranchID == 0) {
            IsValid = false;
            showErrorSnackbar("Select Branch");
        }
        if (IsValid) {
            $.ajax({
                url: Aurl + "purchaseorder/productqtybatch",
                type: 'get',
                contentType: 'json',
                data: { ProdID: nProdID, BranchID: nBranchID },
                xhrFields: {
                    withCredentials: true
                },
                success: function (data) {
                    arrBaseProductBatchInfo = [];
                    modal.style.display = "block";
                    $('#cmbProdBatchUOM').find("option").remove();
                    $.each(data, function (i, items) {
                        $("#lblAbsqty").text(items.ABSQty);
                        $("#lblRTNQty").text(items.SalesReturnPrice);
                        $("#lblDMGRTNQty").text(items.DamageReturnPrice);
                        $("#lblMOH").text(items.MOH);
                        $("#lblMOQ").text(items.MOQ);
                        $("#lblABSValue").text(items.ABSValue);
                        var base = {
                            "ABSQty": items.ABSQty,
                            "SRQty": items.SalesReturnPrice,
                            "DRQty": items.DamageReturnPrice,
                            "MOH": items.MOH,
                            "MOQ": items.MOQ,
                            "ABSValue": items.ABSValue
                        }
                        arrBaseProductBatchInfo.push(base);
                        for (var j = 0; j < items.lstProdQtyBatch.length; j++) {
                            TableBatch.row.add([
                                items.lstProdQtyBatch[j].PurchaseQty,
                                items.lstProdQtyBatch[j].PurchasePrice,
                                items.lstProdQtyBatch[j].SaleQty,
                                items.lstProdQtyBatch[j].TransDate,
                                '<label id="lblPurchaseQty' + j +'" name="PurchaseQty">' + items.lstProdQtyBatch[j].PurchaseQty +'</label>',
                                '<label id="lblPurchasePrice' + j + '" name="PurchasePrice">' + items.lstProdQtyBatch[j].PurchasePrice + '</label>',
                                '<label id="lblSaleQty' + j + '" name="SaleQty">' + items.lstProdQtyBatch[j].SaleQty + '</label>',
                            ]).draw(false);
                        }
                        var purcf = items.UOMList[1].ConvRate;
                        for (var j = 0; j < items.UOMList.length; j++) {
                            $('#cmbProdBatchUOM').append($('<option/>', {
                                value: items.UOMList[j].ID,
                                text: items.UOMList[j].Name,
                                name: items.UOMList[j].ConvRate
                            }));
                            console.log(items.UOMList[j]);
                            if (i == 1)
                                purcf = items.UOMList[j].ConvRate;
                        }
                        $('#cmbProdBatchUOM').val(2);
                        setTimeout(() => {
                            setProductBatchInfo(purcf);
                        }, 300);
                    });
                }
            });
        }
    }
