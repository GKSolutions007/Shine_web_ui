    function calculateProdGrid() {
        $(".lblPT").text(0.00);
        var dPTQty = 0, dPTExcl = 0, dPTIncl = 0, dPTGross = 0, dPTTax = 0, dPTNet = 0, TotProdDisc = 0, TotTradeDisc = 0, TotAddnlDisc = 0, Value = 0;
        $("#tblProductData tbody tr").each(function () {
            var allValues = {};
            $(this).find("input").each(function (index) {
                const fieldName = $(this).attr("name");
                const fieldID = $(this).attr("id");
                if (fieldName == "Qty") {
                    //Value = $(this).val() != "" ? $(this).val() : 0;
                    Value = $(this).attr("txttag");// ReturnTagValue(fieldID, fieldName);
                    dPTQty += parseFloat(Value);
                }
                if (fieldName == "Excl") {
                    //Value = $(this).val() != "" ? $(this).val() : 0;
                    Value = $(this).attr("txttag");//ReturnTagValue(fieldID, fieldName);
                    dPTExcl += parseFloat(Value);
                }
                if (fieldName == "Incl") {
                    //Value = $(this).val() != "" ? $(this).val() : 0;
                    Value = $(this).attr("txttag");// ReturnTagValue(fieldID, fieldName);
                    dPTIncl += parseFloat(Value);
                }
            });
            $(this).find("label").each(function (index) {
                const fieldName = $(this).attr("name");
                const fieldID = $(this).attr("id");
                if (fieldName == "Gross") {
                    dPTGross += parseFloat($(this).attr("lbltag"));//ReturnTagValue(fieldID, fieldName)
                    //dPTGross += parseFloat($(this).text());
                }
                if (fieldName == "TaxAmt") {
                    dPTTax += parseFloat($(this).attr("lbltag"));//parseFloat(ReturnTagValue(fieldID, fieldName));
                    //dPTTax += parseFloat($(this).text());
                }
                if (fieldName == "NetAmt") {
                    dPTNet += parseFloat($(this).attr("lbltag"));//parseFloat(ReturnTagValue(fieldID, fieldName));
                    //dPTNet += parseFloat($(this).text());
                }
                //allValues[fieldName] = $(this).val();
            });
            $("#cmbUOMType").prop("disabled", parseInt(ProductTable.rows().count()) > 0);
        });
        updateLabelColumn("lblPTQty", dPTQty);
        //setTagValue("lblPTQty", "", dPTQty, 2);
        //setTagValue("lblCGST", "", (dPTTax / 2), 2); setTagValue("lblSGST", "", (dPTTax / 2), 2); setTagValue("lblIGST", "", dPTNet, 2);
        updateLabelColumn("lblCGST", (dPTTax / 2)); updateLabelColumn("lblSGST", (dPTTax / 2)); updateLabelColumn("lblIGST", dPTTax);
