                "IsDraft": nIsDraft,
                "ID": $("#txtID").val(),
                "Date": $("#dtpDate").val(),
                "TransMode": nTransMode,
                "TransID": 1,
                "UOMType": $("#cmbUOMType").val(), //1 - Base, 2 - Purchase
                "BranchID": $("#cmbBranch").val(),
                "DocID": $("#lblDocID").val(),
                "VendorID": $("#txtVendorID").val(),
                "RefNo": $("#txtRefNo").val(),
                "Status": nTransMode == 1 || nTransMode == 3 ? 1 : nTransMode == 2 ? 3 : $("#txtStatus").val(),
                "CurrentStatus": $("#txtStatus").val(),
                "CBy": UID,
                "Remarks": $("#txtRemarks").val(),
                "Narration": $("#txtNarration").val(),
                "DraftID": $("#txtDraftID").val() == "" ? 0 : $("#txtDraftID").val(),
                "lstProdGrid": ProductArray,
                "RoundOffAmt": document.getElementById("txtRoundOff").getAttribute("txttag"),// ReturnTagValue("txtRoundOff", ""),//$("#txtRoundOff").val(),
                "GrossAmt": document.getElementById("lblPTGross").getAttribute("lbltag"),//ReturnTagValue("lblPTGross", ""),//$("#lblPTGross").text(),
                "TaxAmt": document.getElementById("lblPTTax").getAttribute("lbltag"),//ReturnTagValue("lblPTTax", ""),//$("#lblPTTax").text(),
                "NetAmt": document.getElementById("lblRoundNet").getAttribute("lbltag"),//ReturnTagValue("lblRoundNet", ""),//$("#lblRoundNet").text(),
            }
            $.ajax({
                url: Aurl + "purchaseorder/save",
                type: 'POST',
                contentType: 'application/json; charset=utf-8',
                data: JSON.stringify(listTrans),
                cache: false,
                processData: false,
                xhrFields: {
                    withCredentials: true
                },
                success: function (data) {
                    $("#btnSave").prop("disabled", false);
                    $.each(data, function (i, items) {
                        if (items.MsgID == 0) {
                            showSuccessSnackbar(items.Message);
                            if (nIsDraft == 0) {
                                //PrintDocument(items.ID, 6);
                                if (nTransMode != 4) {
                                    if (allowprint == 1) {
                                        PrintDocument(items.ID, PrintTransID, PrintConfigID);
                                    } else if (allowprint == 3 || allowprint == 4) {
                                        PrintConfirmDialogMsg(items.ID, PrintTransID, PrintConfigID, "Do you want to print ?");
                                    }
                                }
                                ClearForm();
                            }
                            else {
                                $("#txtDraftID").val(items.ID);
                            }
                        }
                        else {
                            //$("#dvErrorName").html(items.Message);
                            showErrorSnackbar(items.Message);
                        }
                    });
                }, error: function (data) {
                    $("#btnSave").prop("disabled", false);
                    if (data.status == "401") {
                        var msg = data.responseJSON.Message;
                        //showErrorSnackbar(msg + " Logout and Login again.");
                    }
                }
            });
        }
    }
    $("#btnSave").click(function () {
        //SaveData()
        nIsDraft = 0;
        if (cfFocus != 3) {
            ConfirmDialogMsg("Do you want to save data?");
        } else {
            SaveData();
        }
    });
    if ($("#txtID").val() > 0 && $("#txtTransType").val() == 2) {//$("#txtQAViewDocID").val()
        var ID = $("#txtID").val();
        GetProductViewData(7, ID);
        $(".FormclrField,.FormclrcmbField").prop("disabled", true);
        $("#btnSave,#btnClear,#btnDraft").prop("disabled", true);
        $("#btnPreview,#btnPDF,#btnClose").prop("disabled", false);
        $("#dvMain").show();
        $("#dvDocuments").hide();
        $("#MainHeading").text("View " + $("#txtFormName").val());
    }
</script>