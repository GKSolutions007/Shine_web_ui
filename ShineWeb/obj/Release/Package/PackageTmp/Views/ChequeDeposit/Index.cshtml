        setTimeout(function () { GetFilterData(); }, 800);
        $("#MainHeading").text("Cheque Deposit");
        $('#txtDepBankName,#txtDepIFSC').prop("disabled", true);
    }
    function displayResult(item) {
        $('.alert').show().html('You selected <strong>' + item.value + '</strong>: <strong>' + item.text + '</strong>');
    }
    function SaveValidation() {
        var IsValid = true;
        $(".clrdivs").html("");
            if ($("#cmbDepositAccountNo ").val() == 0) {
                IsValid = false;
                $("#divErrDepAccountNo").html("Select Deposit Account");
                showErrorSnackbar("Select Deposit Account");
            }
        if ($("#dtpDate").val() == "") {
            IsValid = false;
            $("#divErrDate").html("Party should not be empty");
            showErrorSnackbar("Party should not be empty");
        }
        if (IsValid) {
            if (DocumentData.rows().count() == 0) {
                IsValid = false;
                showErrorSnackbar("No documents found");
            } else {
                var chkanyone = false;
                $('#tblDocumentData tr td input').each(function (i, items) {
                    if (items.checked) {
                        chkanyone = true;
                    }
                });
                if (!chkanyone) {
                    showErrorSnackbar("Check anyone document");
                    IsValid = false;
                }
            }
        }

        return IsValid;
    }
    var DocArray = [];
    function addDocumentinArray() {
        DocArray = [];
        var nAccID = 0, nCollID = 0, nDepID = 0,nChqStatus = 0;
        $("#tblDocumentData tbody tr").each(function () {
            var allValues = {};
            var chekdoc = false;
            rowdata = DocumentData.row($(this)).data();
            nAccID = rowdata[0];
            nCollID = rowdata[1];
            nDepID = rowdata[2];
            nChqStatus = rowdata[3];
            $(this).find("input[type=checkbox]").each(function (index) {
                chekdoc = $(this).is(":checked");
            });
            if (chekdoc) {
                var listProds = {
                    "AccID": nAccID,
                    "ColID": nCollID,
                    "DepID": nDepID,
                    "Status" : nChqStatus
                }
                DocArray.push(listProds);
            }
        });
    }
    function SaveData() {
        $("#btnSave").prop("disabled", true);
        //showErrorSnackbar("Validation success");
        addDocumentinArray();
        var listTrans = {
            "IsDraft": nIsDraft,
            "ID": $("#txtID").val(),
            "DocDate": $("#dtpDate").val(),
            "TransMode": nTransMode,
            "Docprefix": nDocprefix,
            "DepositAccID": $("#cmbDepositAccountNo").val(),
            "SalesmanID": $("#cmbDepSalesman").val(),
            "Status":$("#txtStatus").val(),
            "CurrentStatus": $("#txtStatus").val(),
            "CBy": UID,
            "chequedepositsdocs": DocArray,
        }
        $.ajax({
            url: Aurl + "chequedeposit/save",
            type: 'POST',
            contentType: 'application/json; charset=utf-8',
            data: JSON.stringify(listTrans),
            cache: false,
            processData: false,
            xhrFields: {
                withCredentials: true
            },
            success: function (data) {
                $("#btnSave").prop("disabled", false);
                $.each(data, function (i, items) {
                    if (items.MsgID == 0) {
                        showSuccessSnackbar(items.Message);
                        ClearForm();
                        setTimeout(function () {
                            GetData(2, 0);
                        }, 500);
                    }
                    else {
                        //$("#dvErrorName").html(items.Message);
                        showErrorSnackbar(items.Message);
                    }
                });
            }, error: function (data) {
                $("#btnSave").prop("disabled", false);
                if (data.status == "401") {
                    var msg = data.responseJSON.Message;
                    ////showErrorSnackbar(msg + " Logout and Login again.");
                }
            }
        });
    }
    $("#btnSave").click(function () {
        //SaveData()
        if (SaveValidation()) {
            if (cfFocus != 3) {
                ConfirmDialogMsg("Do you want to save data?");
            } else {
                SaveData();
            }
        }
    });
