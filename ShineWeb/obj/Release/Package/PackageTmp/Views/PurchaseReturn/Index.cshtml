    function CalculateBatchGrid() {
        let result = [];
        var dTotQty = 0, dFreeTotQty = 0, dDmgTotQty = 0, dPPrice = 0, dQty = 0, dFreeQty = 0, dDmgQty = 0, dQtyTotValue = 0, dDmgQtyTotValue = 0;
        $("#tblBatch tbody tr").each(function () {
            var allValues = {},GridVal =  "";
            $(this).find("input").each(function (index) {
                const fieldName = $(this).attr("name");
                const fieldID = $(this).attr("id");
                GridVal = $(this).val();
                //setTagValue(fieldID, "BatchCalculation", $(this).val());
                if (fieldName == "Qty") {
                    dQty = parseFloat($(this).attr("txttag"));
                    dTotQty += parseFloat(dQty);
                    GridVal = $(this).attr("txttag");
                    //setTagValue(fieldID, fieldName, $(this).val());
                } else if (fieldName == "FreeQty") {
                    dFreeQty = parseFloat($(this).attr("txttag"));
                    dFreeTotQty += parseFloat(dFreeQty);
                    GridVal = $(this).attr("txttag");
                    //setTagValue(fieldID, fieldName, $(this).val());
                } else if (fieldName == "DmgQty") {
                    dDmgQty = parseFloat($(this).attr("txttag"));
                    dDmgTotQty += parseFloat(dDmgQty);//$(this).val()
                    GridVal = $(this).attr("txttag");
                    //setTagValue(fieldID, fieldName, $(this).val());
                }
                else if (fieldName == "ReturnPrice") {
                    dPPrice = parseFloat($(this).attr("txttag"));
                    GridVal = $(this).attr("txttag");
                    //setTagValue(fieldID, fieldName, $(this).val());
                }
                allValues[fieldName] = GridVal;
            });
