    function ReloadPricetypedata(nMode, ProdID, strID,nRowIndex, nUOMID, nCF, dQty, dFreeQty, dDmgQty, dProdDiscPern,
        dTradeDiscPern, dAddnlDiscPern,dTransPrice) {
        $.ajax({
            url: Aurl + "purchasereturn/get",
            type: 'get',
            contentType: 'json',
            data: { Mode: nMode, CodeName: ProdID, ID: strID, BranchID: $("#cmbBranch").val(), Date: $("#dtpDate").val(), "PriceID": $("#cmbPriceType").val() },
            xhrFields: {
                withCredentials: true
            },
            success: function (data) {
                let removeprodarr = BatchAcceptArray.filter(a => a.ProdID != ProdID);
                BatchAcceptArray = removeprodarr;
                var appprice = 0, TotQty = 0, Price = 0, goods = 0, Q1 = 0, Q2 = 0, Q3 = 0, TPren = 0;
                $.each(data, function (i, items) {
                    if (i == 0)
                    {
                        TPren = parseFloat(items.TaxPern);
                        TotQty = parseFloat(dQty) + parseFloat(dDmgQty);
                        appprice = items.ReturnPrice;
                        goods = TotQty * parseFloat(appprice);
                        Q1 = parseFloat(dQty);
                        Q2 = parseFloat(dFreeQty);
                        Q3 = parseFloat(dDmgQty);
                        var PT = {
                            "InventoryID": items.InventoryID,
                            "ProdID": ProdID,
                            "OrgRTNPrice": appprice,
                            "UomID": nUOMID,
                            "TaxPern": items.TaxPern,
                            "TaxID": items.TaxID,
                            "ConversionRate": nCF,
                            "BatchNo": items.BatchNo,
                            "PKDDate": items.PKDDate,
                            "ExpiryDate": items.ExpiryDate,
                            "ActQty": items.ActQty,
                            "ActFreeQty": items.ActFreeQty,
                            "ActDmgQty": items.ActDmgQty,
                            "MRP": items.MRP,
                            "Qty": Q1,
                            "FreeQty": Q2,
                            "DmgQty": Q3,
                            "ReturnPrice": appprice,
                        }
                        BatchAcceptArray.push(PT);
                        return;
                    }
                });
                var PDPern = parseFloat(dProdDiscPern);
                var PDiscAmt = (PDPern * goods) / 100;
                var TDPern = parseFloat(dTradeDiscPern);
                var TDiscAmt = (TDPern * (goods - PDiscAmt)) / 100;
                var ADPern = parseFloat(dAddnlDiscPern);
                var ADiscAmt = (ADPern * (goods - PDiscAmt)) / 100;
                var gross = goods - (PDiscAmt + TDiscAmt + ADiscAmt);
                var TAmt = (gross * TPren) / 100;
                var NAmt = gross + TAmt;
                var DiffPrice = dTransPrice != "" ? dTransPrice : 0;
                $("#tblProductData tbody tr").each(function () {
                    rowdata = ProductTable.row($(this)).data();
                    var RIndex = ProductTable.row($(this)).index();
                    var ProdID = rowdata[0];
                    if (RIndex == nRowIndex) {
                        $(this).find("input").each(function (index) {
                            const fieldName = $(this).attr("name");
                            const fieldID = $(this).attr("id");
                            if (fieldName == "Qty") {
                                updateInputColumn(fieldID, Q1);
                            }
                            if (fieldName == "FreeQty") {
                                updateInputColumn(fieldID, Q2);
                            }
                            if (fieldName == "DmgQty") {
                                updateInputColumn(fieldID, Q3);
                            }
                            if (fieldName == "ProdDiscPern") {
                                updateInputColumn(fieldID, PDPern);
                            }
                            if (fieldName == "ProdDiscAmt") {
                                updateInputColumn(fieldID, PDiscAmt);
                            }
                            if (fieldName == "TradeDiscPern") {
                                updateInputColumn(fieldID, TDPern);
                            }
                            if (fieldName == "TradeDiscAmt") {
                                updateInputColumn(fieldID, TDiscAmt);
