    function getActionsByRole(role, row) {
        let actions = [];
        var rdo1 = $("#rdoMainTableData").is(":checked") ? "1" : $("#rdoDraftTableData").is(":checked") ? "2" : $("#rdoQTNTableData").is(":checked") ? "3" : "4";
        var cancelperm = $("#txtCancelID").val() == 0 ? false : rdo1 == "1" ? true : rdo1 == "2" || rdo1 == "4" ? true : false;

        if (row.Status !== 'Amended' && row.Status !== 'Cancelled') {
            if (rdo1 == "2") {
                if ($("#txtDraftModifyID").val() != 0)
                    actions.push({ cls: "clsEdit", label: "✏️ Edit" });
                if ($("#txtDraftCancelID").val() != 0)
                    actions.push({ cls: "clsCancel", label: "❌ Cancel" });
                if ($("#txtDraftModifyID").val() == 0 && $("#txtDraftCancelID").val() == 0)
                    actions.push({ cls: "", label: "✖️ No Permission found" });
            }
            else {
                if (role.canEdit)
                    actions.push({ cls: "clsEdit", label: "✏️ Edit" });
                if (cancelperm)
                    actions.push({ cls: "clsCancel", label: "❌ Cancel" });
            }
        }

        if (role.canVariant && rdo1 == "1")
            actions.push({ cls: "clsVariant", label: "📄 Variant" });

        if (role.canPrint && rdo1 == "1")
            actions.push({ cls: "clsprint", label: "🖨 Print" });

        if (role.canPreview && rdo1 == "1")
            actions.push({ cls: "clspreview", label: "🖨 Preview" });

        if (role.canPDF && rdo1 == "1")
            actions.push({ cls: "clspdf", label: "📕 PDF" });

        //actions.push({ cls: "clsrowinfo ", label: "ℹ️ Other Info" });
        return actions;
    }


