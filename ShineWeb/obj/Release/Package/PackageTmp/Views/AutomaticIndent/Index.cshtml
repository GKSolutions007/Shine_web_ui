    var ProductArray = [];
    function addProdinArray() {
        ProductArray = [];
        var dPTQty = 0, dPTPrice = 0, dPTABS = 0, dPTMOH = 0, dPTMOQ = 0, nSerial = 1;
        $("#tblProductDataAI tbody tr").each(function () {
            var allValues = {};
            rowdata = ProductTable.row($(this)).data();
            nProdID = rowdata.ID;//[0];
            nUOMID = rowdata.BaseUomID;//[1];

            $(this).find("input").each(function (index) {
                const fieldName = $(this).attr("name");
                if (fieldName == "Qty") {
                    dPTQty = parseFloat($(this).val());
                }
            });

            $(this).find("label").each(function (index) {
                const fieldName = $(this).attr("name");
                if (fieldName == "Price") {
                    dPTPrice = parseFloat($(this).text());
                }
                if (fieldName == "ABSQty") {
                    dPTABS = parseFloat($(this).text());
                }
                if (fieldName == "MOH") {
                    dPTMOH = parseFloat($(this).text());
                }
                if (fieldName == "MOQ") {
                    dPTMOQ = parseFloat($(this).text());
                }
            });
            var listProds = {
                "ID": nProdID,
                "BaseUomID": nUOMID,
                "PurchasePrice": dPTPrice,
                "ABSQty": dPTABS,
                "MOH": dPTMOH,
                "MOQ": dPTMOQ,
                "IndentQty": dPTQty,
                "TrackSerial": nSerial
            }
            ProductArray.push(listProds);
            nSerial++;
        });
    }
    function SaveData() {
        if (SaveValidation()) {
            $("#btnSave").prop("disabled", true);
            //showErrorSnackbar("Validation success");
            addProdinArray();
            var listTrans = {
                "IsDraft": nIsDraft,
                "ID": $("#txtID").val(),
                "Date": $("#dtpDate").val(),
                "TransMode": nTransMode,
                "TransID": 1,
                "BranchID": $("#cmbBranch").val(),
                "DocID": $("#lblDocID").val(),
                "VendorID": $("#txtVendorID").val(),
                "RefNo": $("#txtRefNo").val(),
                "Status": nTransMode == 1 || nTransMode == 3 ? 1 : nTransMode == 2 ? 3 : $("#txtStatus").val(),
                "CurrentStatus": $("#txtStatus").val(),
                "CBy": UID,
                "Remarks": $("#txtRemarks").val(),
                "Narration": $("#txtNarration").val(),
                "DraftID": $("#txtDraftID").val() == "" ? 0 : $("#txtDraftID").val(),
                "lstProduct": ProductArray,
                "TransportID": $("#chkGeneratePO").is(":checked") ? 1 : 0
            }
            $.ajax({
                url: Aurl + "automaticindent/save",
                type: 'POST',
                contentType: 'application/json; charset=utf-8',
                data: JSON.stringify(listTrans),
                cache: false,
                processData: false,
                xhrFields: {
                    withCredentials: true
                },
                success: function (data) {
                    $("#btnSave").prop("disabled", false);
                    $.each(data, function (i, items) {
                        if (items.MsgID == 0) {
                            showSuccessSnackbar(items.Message);
                            if (nIsDraft == 0) {
                                //PrintDocument(items.ID, 6);
                                if (nTransMode != 4) {
                                    if (allowprint == 1) {
                                        PrintDocument(items.ID, PrintTransID, PrintConfigID);
                                    } else if (allowprint == 3 || allowprint == 4) {
                                        PrintConfirmDialogMsg(items.ID, PrintTransID, PrintConfigID, "Do you want to print ?");
                                    }
                                }
                                ClearForm();
                            }
                            else {
                                $("#txtDraftID").val(items.ID);
                            }
                        }
                        else {
                            //$("#dvErrorName").html(items.Message);
                            showErrorSnackbar(items.Message);
                        }
                    });
                }, error: function (data) {
                    $("#btnSave").prop("disabled", false);
                    if (data.status == "401") {
                        var msg = data.responseJSON.Message;
                        ////showErrorSnackbar(msg + " Logout and Login again.");
                    }
                }
            });
        }
    }
    $("#btnSave").click(function () {
        //SaveData()
        nIsDraft = 0;
        if (cfFocus != 3) {
            ConfirmDialogMsg("Do you want to save data?");
        } else {
            SaveData();
        }
    });

