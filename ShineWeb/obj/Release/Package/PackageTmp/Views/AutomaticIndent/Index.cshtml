    function SaveValidation() {
        var IsValid = true;
        $(".clrdivs").html("");
        if ($("#txtSearch").val() == "") {
            IsValid = false;
            $("#divErrSearch").html("Party should not be empty");
            showErrorSnackbar("Party should not be empty");
        }
        else {
            const exists = arrVendorName.indexOf($("#txtSearch").val().trim()) !== -1;
            if (!exists) {
                IsValid = false;
                $("#divErrSearch").html("Select Party Name from list");
                showErrorSnackbar("Select Party Name from list");
            }
        }
        if (nIsDraft == 0) {
            if ($("#cmbBranch ").val() == 0) {
                IsValid = false;
                $("#divErrBranch").html("Select Branch");
                showErrorSnackbar("Select Branch");
            }
        }
        if ($("#dtpDate").val() == "") {
            IsValid = false;
            $("#divErrDate").html("Party should not be empty");
            showErrorSnackbar("Party should not be empty");
        }
        if (parseFloat($("#lblPTQty").text()) <= 0) {
            IsValid = false;
            //$("#divErrDate").html("Party should not be empty");
            showErrorSnackbar("Total Qty should be greater than Zero");
            showErrorSnackbar("Total Qty should be greater than Zero");
        }
        if (ProductTable.rows().count() == 0) {
            IsValid = false;
            $("#divErrProduct").html("Add atleat one product in Grid");
            showErrorSnackbar("Add atleat one product in Grid");
        }
        return IsValid;
    }
