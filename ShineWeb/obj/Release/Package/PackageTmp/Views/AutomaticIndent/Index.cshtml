    function calculateProdGrid() {
        $(".lblPT").text(0.00);
        var dPTQty = 0, dPTNet = 0, Price = 0;
        $("#tblProductDataAI tbody tr").each(function () {
            var allValues = {};
            $(this).find("label").each(function (index) {
                const fieldName = $(this).attr("name");
                if (fieldName == "Price") {
                    Price = parseFloat($(this).text());
                }
                //allValues[fieldName] = $(this).val();
            });
            $(this).find("input").each(function (index) {
                const fieldName = $(this).attr("name");

                if (fieldName == "Qty") {
                    Value = $(this).val() != "" ? parseFloat($(this).val()) : 0;
                    dPTQty += Value;
                    dPTNet += (Price * Value)
                }
            });

        });
        $("#lblRoundNet").text(dPTNet.toFixed(2));
        $("#lblPTQty").text(dPTQty.toFixed(2));
        //$("#lblPTCount").text(parseInt(ProductTable.rows().count()));
    }
