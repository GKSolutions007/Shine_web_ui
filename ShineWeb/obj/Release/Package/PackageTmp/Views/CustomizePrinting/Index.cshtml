<body>
    @Html.HiddenFor(model => model.FormName, new { @autocomplete = "off", @class = "form-control", @id = "txtFormName" })
    @Html.HiddenFor(model => model.ID, new { @autocomplete = "off", @class = "form-control FormclrField", @id = "txtID" })
    @*<h4 id="MainHeading">@ViewData["FormName"]</h4>
        <hr />*@
    <div class="toastBox"></div>
    @*<div class="qrcodeheader"></div>
        <div class="barcodeheader"></div>*@
    <div class="row" style="background-color:lightcoral">
        <div id="controls" style="padding-top:10px;">
            <table>
                <tr>
                    <td width="55%">
                        <select id="paperType" style="max-width: 250px; height: 30px; border-radius: 10px;">
                            <option value="A4">A4</option>
                            <option value="A5">A5</option>
                            <option value="A5L">A5 - Landscape</option>
                            <option value="custom">Custom</option>
                        </select>
                        <input type="number" id="customWidth" placeholder="Width mm" style="max-width: 75px; height: 30px; border-radius: 10px;">
                        <input type="number" id="customHeight" placeholder="Height mm" style="max-width: 75px; height: 30px; border-radius: 10px;">
                        <select id="cmbFontNames" style="max-width: 200px; height: 30px; border-radius: 10px;">
                            <option value="1">Times New Roman</option>
                            <option value="2">sanserif</option>
                            <option value="3">Pixel</option>
                        </select>
                        <input type="color" style="max-width: 25px; height: 30px; border-radius: 10px;" id="txtColors"/>
                        <input type="number" id="txtFontsize" placeholder="Size" value="8" style="max-width: 75px; height: 30px; border-radius: 10px;">
                        <label class="checkbox-label" title="Continues Paper">
                            <input id="chkContinuesPapper" type="checkbox" checked>
                            CP
                        </label>&emsp;
                        <label class="checkbox-label" title="Footer on Every Pages">
                            <input id="chkFooteronEveryPage" type="checkbox" />
                            FEP
                        </label>&emsp;
                        <label class="checkbox-label">
                            <input id="chkIncut" type="checkbox">
                            Inc Cut:
                        </label>&emsp;
                        <button class="edit-icon-button" id="editBtn">
                            <img src="~/Images/pencil-case.png" />
                        </button>
                    </td>
                    <td style="padding-left:5px;width:6%">
                        <span class="form-label">
                            <input class="FormclrField" type="number" id="txtItemsPerPage" value="10" style="max-width: 105px; height: 35px; border-radius: 10px;" placeholder=""
                                   maxlength="2" oninput="javascript: if (this.value.length > this.maxLength) { this.value = this.value.slice(0, this.maxLength); }">
                            <label class="labelup">Item Per Page</label>
                        </span>
                    </td>
                    <td style="padding-left:5px;width:2%">
                        <span class="form-label" title="Line Feed">
                            <input class="FormclrField" type="text" value="2" id="txtLF" placeholder="" style="max-width: 55px; height: 35px; border-radius: 10px;"
                                   maxlength="1" oninput="javascript: if (this.value.length > this.maxLength) { this.value = this.value.slice(0, this.maxLength); }">

                            <label class="labelup">LF</label>
                        </span>
                    </td>
                    <td style="padding-left:5px;width:2%">
                        <span class="form-label" title="Bottom Line Space">
                            <input class="FormclrField" value="2" type="text" id="txtBLS" placeholder="" style="max-width: 55px; height: 35px; border-radius: 10px;"
                                   maxlength="1" oninput="javascript: if (this.value.length > this.maxLength) { this.value = this.value.slice(0, this.maxLength); }">
                            <label class="labelup">BLS</label>
                        </span>
                    </td>
                    <td style="padding-left:5px;width:12%">
                        <label><input type="radio" name="PrinterType" id="rdoWindows" checked> Windows</label>&emsp;
                        <label><input type="radio" name="PrinterType" id="rdoDOS"> DOS</label>
                    </td>
                </tr>
