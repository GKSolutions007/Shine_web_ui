<script type="text/javascript">
     var UID = '@Session["LoginUserID"]';
 var nMode = 1;
    var FID = $("#txtFormID").val();
    //  showErrorSnackbar(FID);
    //4- creditterm,17-gift,16-tax comp
    if (FID == 17) {
        $("#lbltext").text("Points *");
        $("#trtdlbl").text("Points");
    } else {
        $("#lbltext").text("Value *");
        $("#trtdlbl").text("Value");
    }
    var Table = $('#tblMasterData').DataTable({pageLength: ItemsperPage,lengthMenu: [[10, 20, 50, 100, -1], [10, 20, 50, 100, "All"]],
        "bFilter": true,
        "bSearchable": true,
        "bInfo": true,
        "bPaginate": true,
        "ordering": true,
        scrollY: "400px",
        scrollX: true,
        scrollCollapse: true,
        "columnDefs": [{ "targets": [0, 6], "visible": false, searchable: false }, { "targets": [3, 5, 4], "class": "text-center" }
            , { "targets": [1], "width": "60%" }, { "targets": [2], "width": "10%" }, { "targets": [2], "width": "10%" }, { "targets": [4,5], "width": "10%" }],
    });
    var DecVal = '@Session["DecimalValues"]';
    var Aurl = '@Session["APIurl"]';
    if ($("#txtAddID").val() == 0) {
        $("#trAdd").hide();
        $("#btnAdd").hide();
    }
    if ($("#txtModifyID").val() == 0) {
        Table.column(4).visible(false);
    }
    if ($("#txtViewID").val() == 0) {
        //$(".clsVariant").hide();
        Table.column(5).visible(false);
    }
    if ($("#txtModifyID").val() == 0 && $("#txtViewID").val() == 0 && $("#txtAddID").val() == 1) {
        $("#dvMainTable").hide();
    }
    var arrData = [];
    GetData();
    function GetData() {
        $.ajax({
            url: Aurl + "taxcompgift/get",
            /*url: "http://localhost:44396/api/singlemaster/get",*/
            type: 'get',
            contentType: 'json',
            data: { FormID: FID, Mode: 3, Name: "" },
            xhrFields: {
                withCredentials: true
            },
            success: function (data) {
                if (data.length > 0) {
                    arrData = [];
                    //Table.draw().clear();
                    $.each(data, function (i, items) {
                        //arrData[i] = items.Name
                        var al = {
                            "label": items.Name,
                            "active": items.Active,
                            "id": items.ID,
                            "value": items.Value
                        }
                        arrData.push(al);
                        //Table.row.add([
                        //    items.ID,
                        //    items.Name,
                        //    FID != 16 ? parseInt(items.Value) : parseFloat(items.Value).toFixed(DecVal),
                        //    items.Active == "True" ? "Yes" : "No",
                        //    '<a href="javascript:void(0) style="cursor: pointer;"><span class="glyphicon glyphicon-pencil clsEdit" style="font-size:16px" title="Edit"></span></a>',
                        //    '<a href="javascript:void(0) style="cursor: pointer;"><span class="glyphicon glyphicon-eye-open clsView" style="font-size:16px" title="View"></span></a>',
                        //    FID != 16 ? parseInt(items.Value) : parseFloat(items.Value),
                        //]).draw(false);
                    });
                    var searchValue = '';
                    if ($.fn.DataTable.isDataTable('#tblMasterData')) {
                        searchValue = $('#tblMasterData').DataTable().search(); // get current search term
                    }
                    $('#tblMasterData').DataTable({pageLength: ItemsperPage,lengthMenu: [[10, 20, 50, 100, -1], [10, 20, 50, 100, "All"]],
                        scrollY: "450px",
                        scrollX: true,
                        scrollCollapse: true,
                        paging: true,
                        destroy: true,
                        data: data,
                        fixedColumns: false,
                        searching: true,
                        ordering: true,
                        sorting: false,
                        columns: [
                            { "data": "ID", "autoWidth": true, "visible": false, className: "dt-body-left" },
                            { "data": "Name", "autoWidth": true, className: "dt-body-left" },
                            {
                                className: "dt-body-center",
                                "mRender": function (data, type, value) {
                                    var _action = FID != 16 ? parseInt(value.Value) : parseFloat(value.Value).toFixed(DecVal);// '<a href="javascript:void(0) style="cursor: pointer;"><span class="glyphicon glyphicon-pencil clsEdit" style="font-size:16px" title="Edit"></span></a>';
                                    return _action;
                                }
                            },
                            {
                                className: "dt-body-center",
                                "mRender": function (data, type, value) {
                                    var _action = value.Active == "True" ? "Yes" : "No";// '<a href="javascript:void(0) style="cursor: pointer;"><span class="glyphicon glyphicon-pencil clsEdit" style="font-size:16px" title="Edit"></span></a>';
                                    return _action;
                                }
                            },
                            {
                                className: "dt-body-center",
                                "mRender": function (data, type, value) {
                                    var _action = '<label title="' + value.CDate + '">' + value.CBy + '</label>';
                                    return _action;
                                }
                            },
                            {
                                className: "dt-body-center",
                                "mRender": function (data, type, value) {
                                    var _action = '<a href="javascript:void(0) style="cursor: pointer;"><span class="glyphicon glyphicon-pencil clsEdit" style="font-size:16px" title="Edit"></span></a>';
                                    return _action;
                                }, "visible": $("#txtModifyID").val() == 0 ? false : true
                            },
                            {
                                className: "dt-body-center",
                                "mRender": function (data, type, value) {
                                    var _action = '<a href="javascript:void(0) style="cursor: pointer;"><span class="glyphicon glyphicon-eye-open clsView" style="font-size:16px" title="View"></span></a>';
                                    return _action;
                                }, "visible": $("#txtViewID").val() == 0 ? false : true
                            },
                            {
                                className: "dt-body-center",
                                "mRender": function (data, type, value) {
                                    var _action = FID != 16 ? parseInt(value.Value) : parseFloat(value.Value).toFixed(DecVal);// '<a href="javascript:void(0) style="cursor: pointer;"><span class="glyphicon glyphicon-pencil clsEdit" style="font-size:16px" title="Edit"></span></a>';
                                    return _action;
                                }, "visible":false
                            },
                        ],
                        initComplete: function () {
                            // Re-apply search value after table is fully initialized
                            this.api().search(searchValue).draw();
                        }
                    });
                    Table = $('#tblMasterData').DataTable();
                    $("#txtName").autocomplete({
                        minLength: 0,
                        source: arrData,
                        select: function (event, ui) {
                            $("#txtName").val(ui.item.label);
                            return false;
                        },
                    }).autocomplete("instance")._renderItem = function (ul, item) {
                        return $("<li class='each'>")
                            .append("<div class='acItem'><span class='name'>" +
                                item.label + "</span></div>")
                            .appendTo(ul);
                    };
                    setTimeout(function () {
                        $("#txtName").val("");
                        $('input#txtName').autocomplete("search");
                        var esc = $.Event("keydown", { keyCode: 27 });
                        $("#txtName").trigger(esc);
                    }, 100);
                }
                else {
                    showErrorSnackbar("No Records Found");
                }
            },
            error: function (data) {
                if (data.status == "401") {
                    var msg = data.responseJSON.Message;
                    ////showErrorSnackbar(msg + " Logout and Login again.");
                }
            }
        });
    }
    function updateInputColumn(inputID, netValue) {
        //const label = document.getElementById(`dgvProdGross${rowId}`);
        const input = document.getElementById(inputID);
        var value = netValue != "" ? netValue : 0;
        input.setAttribute('txttag', parseFloat(value).toFixed(4));
        input.value = parseFloat(value).toFixed(DecVal);
        input.title = netValue;
    }
    $(".txtsettag").focusin(function (e) {
        var id = e.target.id;
        if (FID == 16) {
            const val = document.getElementById("txtValue").getAttribute("txttag");       
            $("#" + id).val(val);
        }
        $("#" + id).select();
    });
    $(".txtsettag").blur(function (e) {
        var val = e.target.value;
        var id = e.target.id;
        if (FID == 16)
            updateInputColumn(id, val);
    });
    function isNumberKey(evt) {
        if (FID != 16) {
            var charCode = (evt.which) ? evt.which : evt.keyCode
            if (charCode > 31 && (charCode < 48 || charCode > 57))
                return false;
            return true;
        }
    }
    function displayResult(item) {
        $('.alert').show().html('You selected <strong>' + item.value + '</strong>: <strong>' + item.text + '</strong>');
    }
    var modal = document.getElementById("myModalfg");
    var span = document.getElementsByClassName("closefg")[0];
    span.onclick = function () {
        modal.style.display = "none";
    }
    $('#btnClose').click(function (e) {
        if (ClosecfFocus != 3) { CloseConfirmDialogMsg("Do you want to close?"); } else { CloseForm(); }
    });
    function CloseForm() {
        modal.style.display = "none";        
    }
    $("#btnAdd").click(function () {
        nMode = 1;
        $("#chkActive,#btnSave,#txtName,#btnClear,#txtValue").prop("disabled", false);
        $(".modal-title#lblPopupheading").text("Add " + $("#MainHeading").text());
        modal.style.display = "block";
        ClearForm();
    });
    $('#btnClear').click(function (e) {
        if (cfFocus != 3) { ClearConfirmDialogMsg("Do you want to clear?"); } else { ClearForm(); }
    });
    function ClearForm() {
        $("#txtName,#txtValue").val("");
        $("#chkActive").prop("checked", true);
        $("#chkActive").prop("disabled", true);
        $(".clrdiv").html("");
        $("#txtName").focus();
        if (FID == 16)
            updateInputColumn("txtValue", 0);
        if (nMode != 1)
            modal.style.display = "none";
    }
    $("#btnSave").click(function () {
        $(".clrdiv").html("");
        var IsValid = true;
        if($("#txtName").val() == "") {
            $("#dvErrorName").html("Name should not be empty");
            showErrorSnackbar("Name should not be empty");
            IsValid = false;
        }
        if ($("#txtValue").val() == "") {
            $("#dvErrorValue").html("Value should not be empty");
            showErrorSnackbar("Value should not be empty");
            IsValid = false;
        }
        if (IsValid) {
            if (cfFocus != 3) { ConfirmDialogMsg("Do you want to save data?"); } else { SaveData(); }
        }
    });
    function SaveData() {
        $(".clrdiv").html("");
        $("#btnSave").prop("disabled", true);
        var Val = FID == 16 ? document.getElementById("txtValue").getAttribute("txttag") : $("#txtValue").val();
            var lstMaster = {
                "FormID": FID,
                "Mode": nMode,
                "ID": $("#txtID").val(),
                "Name": $("#txtName").val(),
                "Value": Val,
                "Active": $("#chkActive").is(":checked") ? 1 : 0,
                "UserID": UID
            };
            $.ajax({
                url: Aurl + "taxcompgift/save",
                type: 'POST',
                contentType: 'application/json; charset=utf-8',
                data: JSON.stringify(lstMaster),
                cache: false,
                processData: false,
                xhrFields: {
                    withCredentials: true
                },
                success: function (data) {
                    $("#btnSave").prop("disabled", false);
                    $.each(data, function (i, items) {
                        if (items.MsgID == 0) {
                            //showErrorSnackbar(items.Message);
                            showSuccessSnackbar(items.Message);
                            ClearForm();
                            GetData();
                            modal.style.display = "none";
                        }
                        else {
                            $("#dvErrorName").html(items.Message);
                            showErrorSnackbar(items.Message);
                        }
                    });
                },
                error: function (data) {
                    $("#btnSave").prop("disabled", false);
                    if (data.status == "401") {
                        var msg = data.responseJSON.Message;
                        ////showErrorSnackbar(msg + " Logout and Login again.");
                    }
                }
            });
    }
    $('#tblMasterData tbody').on('click', '.clsView', function () {
        var table = $('#tblMasterData').DataTable();
        var data = table.row($(this).parents('tr')).data();
        console.log("Row clicked data:", data);
        rowdata = Table.row($(this).parents('tr')).data();
        var ID = rowdata.ID;//[0];
        var Name = rowdata.Name;//[1];
        var Value = rowdata.Value;//[2];
        var Active = rowdata.Active;//[3];
        var OrgVal = rowdata.Value;//[6];
        nMode = 3;
        //if (Table.cell(this).index().columnVisible == 3)
        {
            $(".modal-title#lblPopupheading").text("View " + $("#MainHeading").text());
            $("#txtID").val(ID);
            $("#txtName").val(Name);
            $("#txtValue").val(parseInt(Value));
            if (FID == 16)
                updateInputColumn("txtValue", OrgVal);
            $("#chkActive").prop("checked", Active == "True");
            $("#chkActive,#btnSave,#txtName,#txtValue").prop("disabled", true);
            $("#btnClear").prop("disabled", true);
            $(".clrdiv").html("");
            modal.style.display = "block";
        }
    });

    $('#tblMasterData tbody').on('click', '.clsEdit', function () {
        rowdata = Table.row($(this).parents('tr')).data();
        var ID = rowdata.ID;//[0];
        var Name = rowdata.Name;//[1];
        var Value = rowdata.Value;//[2];
        var Active = rowdata.Active;//[3];
        var OrgVal = rowdata.Value;//[6];
        //if (Table.cell(this).index().columnVisible == 2)
        {
            nMode = 2;
            $(".modal-title#lblPopupheading").text("Edit " + $("#MainHeading").text());
            $("#txtID").val(ID);
            $("#txtName").val(Name);
            $("#txtValue").val(parseInt(Value));
            if (FID == 16)
                updateInputColumn("txtValue", OrgVal);
            $("#chkActive").prop("checked", Active == "True");
            $("#chkActive,#btnSave,#txtName,#txtValue").prop("disabled", false);
            $("#btnClear").prop("disabled", true);
            $(".clrdiv").html("");
            modal.style.display = "block";
        }
    });
    @*var cfFocus = '@Session["ConfirmFocus"]';
    function ConfirmDialogMsg(message) {
        var IsAccepted = false;
        $(".ui-dialog-titlebar-close").hide();
        $('<div></div>').appendTo('body')
            .html('<div><h6>' + message + '</h6></div>')
            .dialog({modal: true,closeOnEscape: true,title: 'Confirm Popup',zIndex: 100,autoOpen: true,
                width: '300px',
                resizable: true,
                close: false,
                open: function () {
                    $(this).closest(".ui-dialog")
                        .find(".ui-dialog-titlebar-close")
                        .removeClass("ui-dialog-titlebar-close")
                        .html("x");
                },
                buttons: [
                    {
                        text: "Yes",
                        class: "btn btn-success clsYes",
                        click: function () {
                            $(this).dialog("close");
                            SaveData();
                        },
                    },
                    {
                        text: "No",
                        class: "btn btn-danger clsNo cfforeclr",
                        click: function () {
                            $(this).dialog("close");
                        }
                    }
                ],
            });
        if (cfFocus == 1) {
            $(".clsYes").focus();
        }
        else {
            $(".clsNo").focus();
        }
