<body>
    @Html.HiddenFor(model => model.FormName, new { @autocomplete = "off", @class = "form-control", @id = "txtFormName" })
    @Html.HiddenFor(model => model.ID, new { @autocomplete = "off", @class = "form-control FormclrField", @id = "txtID" })
    @Html.HiddenFor(model => model.TransType, new { @autocomplete = "off", @class = "form-control", @id = "txtTransType" })
    @Html.HiddenFor(model => model.EnableReturnPrice, new { @autocomplete = "off", @class = "form-control", @id = "txtReurnPricePerm" })
    <h4 id="MainHeading">@ViewData["FormName"]</h4>
    <hr />
    <div class="toastBox"></div>
    <div id="dvMain">        
            <div class="row">
                <div class="col-sm-12 col-md-12 col-lg-12">
                    <table width="100%">
                        <tr hidden>
                            <td>
                                <input hidden id="txtHProdID" class="FormclrField" />
                                <input hidden id="txtHTrackInventory" />
                                <input hidden id="txtHTrackBatch" />
                                <input hidden id="txtHPKD" />
                                <input hidden id="txtHTaxPern" />
                            </td>
                        </tr>
                        <tr>
                            <td style="border:none;" colspan="2" width="20%">
                                <div>
                                    <span class="form-label">
                                        <select class="form-control FormclrcmbField" id="cmbBranch"><option value="0">Select Branch</option></select>
                                        <label class="labelup" for="txtCode"> Branch *</label>
                                    </span>
                                    <div style="color: red; padding-top: 5px; padding-left: 5px;" id="divErrBranch" class="clrdivs"></div>
                                </div>
                            </td>
                            <td style="border:none;" width="30%" class="trItemName">
                                <span class="form-label">
                                    <input class="form-control FormclrField" id="txtProdName" />
                                    <input id="txtHProdCode" hidden />
                                    <label class="labelup" for="txtCode">Item Details </label>
                                </span>
                                <label class="form-label" style="padding-left: 1em" id="lblProdCodeAS"></label>
                                <div style="color: red; padding-top: 5px; padding-left: 5px;" id="divErrProduct" class="clrdivs"></div>
                            </td>
                            <td style="border:none;" width="10%" class="trItemName">
                                <input style="font-size:14px;width:14px;height:14px;" class="FormfilterclrField" type="checkbox" id="chkInclusive">
                                <label for="chkInclusive" style="font-size:14px;">Inclusive</label>
                            </td>
                            <td style="border:none;" width="10%" class="trItemName">
                                <span class="form-label">
                                    <input class="form-control FormclrField" id="txtUOMName" />
                                    <label class="labelup">UOM </label>
                                </span>
                            </td>
                            <td style="border:none;" width="10%" class="trItemName">
                                <span class="form-label">
                                    <input class="form-control FormclrField" id="txtTaxName" />
                                    <label class="labelup">Tax % </label>
                                </span>
                            </td>
                            <td width="20%" style="border:none;font-size:12px">
                               Location : <label id="lblLocationName"></label>
                            </td>
                        </tr>
                    </table>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12 col-md-12 col-lg-12" style="padding-right:35px;">
                    <table>
                        <tr>
                            <th colspan="9">
                            </th>
                            <th title="Sales Margin">
                                <input min="0" txttag="0" title="0" type="number" step="any" style="border:none !important" id="txtSalesMargin" name="SalesMargin" class="gdInput txtsettag" value="0.00" disabled />
                            </th>
                            <th title="ECP Margin">
                                <input min="0" txttag="0" title="0" type="number" step="any" style="border:none !important" id="txtECPMargin" name="ECPMargin" class="gdInput txtsettag" value="0.00" disabled />
                            </th>
                            <th title="SPL Margin">
                                <input min="0" txttag="0" title="0" type="number" step="any" style="border:none !important" id="txtSPLMargin" name="SPLMargin" class="gdInput txtsettag" value="0.00" disabled />
                            </th>
                            <th colspan="3">
                            </th>
                        </tr>
                    </table>
                    <table class="table table-bordered  row-border order-column display" id="tblProductData" width="100%">
                        <thead>                            
                            <tr>
                                <th style="text-align:center;font-size: 12px;width:0%;" class="trclrforcolheader">InventoryID</th>
                                <th style="text-align:center;font-size: 12px;width:3%;" class="trclrforcolheader"><span class="glyphicon glyphicon-check"></span></th>
                                <th style="text-align:center;font-size: 12px;width:2%;" class="trclrforcolheader">#</th>
                                <th style="text-align:center;font-size: 12px;width:5%;" class="trclrforcolheader">Trans Date</th>
                                <th style="text-align:center;font-size: 12px;width:9%;" class="trclrforcolheader">Batch No</th>
                                <th style="text-align:center;font-size: 12px;width:10%;"class="trclrforcolheader">PKD</th>
                                <th style="text-align:center;font-size: 12px;width:10%;"class="trclrforcolheader">Expiry</th>
                                <th style="text-align:center;font-size: 12px;width:8%;" class="trclrforcolheader">ABS Qty</th>
                                <th style="text-align:center;font-size: 12px;width:8%;" class="trclrforcolheader">Purchase ₹</th>
                                <th style="text-align:center;font-size: 12px;width:9%;" class="trclrforcolheader">Sales ₹</th>
                                <th style="text-align:center;font-size: 12px;width:9%;" class="trclrforcolheader">ECP ₹</th>
                                <th style="text-align:center;font-size: 12px;width:9%;" class="trclrforcolheader">SPL ₹</th>
                                <th style="text-align:center;font-size: 12px;width:9%;" class="trclrforcolheader">Return ₹</th>
                                <th style="text-align:center;font-size: 12px;width:9%;" class="trclrforcolheader">MRP Incl ₹</th>
                                <th style="text-align:center;font-size: 12px;width:9%;" class="trclrforcolheader">MRP Excl ₹</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                        <tfoot id="trFooter">
                            <tr>
                                <td align="right" colspan="7">
                                    <label> Total :</label>
                                </td>
                                <td align="right"><label id="lblPTQty" style="text-align:right" class="lblPT">0.00</label></td>
                                <td align="right" colspan="7"></td>
                            </tr>
                        </tfoot>
                    </table>
                </div>
            </div>
            @*<button id="btnProdAdd" class="btn btn-info"><span class="glyphicon glyphicon-plus" style="color: white"></span>&#x2009;Add</button>*@
        

        <div class="footerAction modal-footer">
            <div style="padding-right:20px" align="right">
                <button type="button" class="btn btn-danger" id="btnClear"><span class="glyphicon glyphicon-refresh"> Clear</span></button>
                <button type="button" class="btn btn-success" id="btnSave"><span class="glyphicon glyphicon-saved"> Save</span></button>
            </div>
        </div>
    </div>
</body>



<script type="text/javascript">
    //document.body.style.zoom = "85%";
    $(".FormBtField").prop("disabled", true);
    var ProductTable = $('#tblProductData').DataTable({
        paging: false,
        searching: false,
        info: false,
        ordering: true,
        scrollY: "350px",
        scrollX: true,
        scrollCollapse: true,
        order: [[1, 'asc']],
        "columnDefs": [{ "targets": [0], "visible": false, searchable: false, ordering: false }, { "targets": [1], "class": "text-center" }
            , { "targets": [3,4, 5, 6, 7, 10, 8, 9, 11,12,13,14], "width": "7%" }, { "targets": [1,2], "width": "2%" }],
    });
    $(".trItemName").prop("disabled", true);
    var UID = '@Session["LoginUserID"]';
    var Aurl = '@Session["APIurl"]';
    var DecVal = '@Session["DecimalValues"]';
    var DefBranch = '@Session["DefaultBranch"]';
    var BatchMode = 0;
    var nTransMode = 1, nIsDraft = 0;
    var qtyonly = $("#chkQtyonly").is(":checked") ? 1 : 0;
    //GetData(1);
    var RtnDisp = 1;
    if ($("#txtReurnPricePerm").val() == 0) {
        RtnDisp = 0;
    }
    $('input[type=number]').focusin(function (e) {
        $(this).select();
    });
    
        function SetTagZero() {
        var inputs = $(".txtsettag");
        for (var i = 0; i < inputs.length; i++) {
            var field = $(inputs[i]);
            var FID = field[0].id;
            updateInputColumn(FID, "0");
        }
    }
function updateInputColumn(inputID, netValue) {
    //const label = document.getElementById(`dgvProdGross${rowId}`);
    const input = document.getElementById(inputID);
    var value = netValue != "" ? netValue : 0;
    input.setAttribute('txttag', parseFloat(value).toFixed(4));
    input.value = parseFloat(value).toFixed(DecVal);
    input.title = netValue;
    }
    function updateLabelColumn(labelID, netValue) {
        //const label = document.getElementById(`dgvProdGross${rowId}`);
        const input = document.getElementById(labelID);
        var value = netValue != "" ? netValue : 0;
        input.setAttribute('lbltag', parseFloat(value).toFixed(4));
        input.innerText = parseFloat(value).toFixed(DecVal);
        input.title = netValue;
    }
    $(".txtsettag").focusin(function (e) {
        var id = e.target.id;
        const val = document.getElementById(id).getAttribute("txttag");
        $("#" + id).val(val);
        $("#" + id).select();
    });
    $(".txtsettag").blur(function (e) {
        var val = e.target.value;
        var id = e.target.id;
        updateInputColumn(id, val);
    });
    $('#tblProductData tbody').on('focusin', 'td>.txtsettag', function (e) {
        // .clsdgvTag
        var id = e.target.id;
        var name = e.target.name;
        var value = e.target.value;
        //getTagValue(id, name);
        const val = document.getElementById(id).getAttribute("txttag");
        $("#" + id).val(val);
        $("#" + id).select();
        batgridchanges = false;
    });
    var batgridchanges = false;
    $('#tblProductData tbody').on('blur', 'td>.txtsettag', function (e) {
        // .clsdgvTag
        var id = e.target.id;
        var name = e.target.name;
        var value = e.target.value;
        //setTagValue(id, name, value);
        if (!batgridchanges) {
            updateInputColumn(id, e.target.value);
        }
        //CalculateBatchGrid();
    });
    var BatchRID = 0, ProductRID = 0;

    function ReorderSerialNo() {
        var sNo = 0;
        $("#tblProductData tbody tr").each(function () {
            $(this).find("p").each(function (index) {
                vals = (sNo + 1);
                $(this).text(vals);
            });
            sNo++;
        });
    }
    $("#tblProductData").change(function (e) {
        var id = e.target.id;
        var name = e.target.name;
        var val = e.target.value;
        var inptype = e.target.type;
        batgridchanges = true;
        var SalesMargin = parseFloat($("#txtSalesMargin").attr("txttag"));
        var ECPMargin = parseFloat($("#txtECPMargin").attr("txttag"));
        var SPLMargin = parseFloat($("#txtSPLMargin").attr("txttag"));
        if (name != "BatchNo" && name != "PKD" && name != "Expiry" && name != "CheckBatch" && name != "MRP")
        {
            updateInputColumn(id, val);
        }
        if (name == "CheckBatch")
        {
            $("#tblProductData tbody tr").each(function () {
                var allValues = {};
                var chk = true;
                $(this).find("input").each(function (index) {
                    const fieldName = $(this).attr("name");
                    const fieldID = $(this).attr("id");
                    if (fieldName == "CheckBatch") {
                        chk = $(this).is(":checked");//.val();
                    }
                    else
                    {                        
                        if (fieldName != "PurchasePrice" && fieldName != "ActQty" && fieldName != "ReturnPrice" && fieldName != "MRPExcl") {
                            $(this).prop("disabled", !chk);
                        } else if (fieldName == "ReturnPrice" && RtnDisp == 1) {
                            $(this).prop("disabled", !chk);
                        }                  
                    }
                });
            });
        }
    });
    //
    $('#tblProductData tbody').on('change', '.clsCheckBatch', function (e) {
        var chk = $(this).is(":checked");
        var isInclusiveChk = $("#chkInclusive").is(":checked");
        var currrowdata = ProductTable.row(nCurrentRowIndex).data();
        var currrowindex = ProductTable.row(nCurrentRowIndex).index();
        var SalesMargin = parseFloat($("#txtSalesMargin").attr("txttag"));
        var ECPMargin = parseFloat($("#txtECPMargin").attr("txttag"));
        var SPLMargin = parseFloat($("#txtSPLMargin").attr("txttag"));
        $("#tblProductData tbody tr").each(function () {
            var rowindex = ProductTable.row($(this)).index();
            var PurchasePrice = 0, InclMRP = 0, ExclMRP = 0;
            var tp = parseFloat($('#txtHTaxPern').val());
            if (currrowindex == rowindex) {
                $(this).find("input[name=MRP]").each(function (index) {
                    InclMRP = parseFloat($(this).attr("txttag"));
                });                
                $(this).find("input[name=MRPExcl]").each(function (index) {
                    ExclMRP = parseFloat($(this).attr("txttag"));
                }); 
                $(this).find("input").each(function (index) {
                    const fieldName = $(this).attr("name");
                    const fieldID = $(this).attr("id");
                    if (fieldName == "PurchasePrice") {
                        PurchasePrice = parseFloat($(this).attr("txttag"));
                    }
                    if (fieldName == "SalePrice") {
                        var SalesPrice = parseFloat($(this).attr("txttag"));
                        if (chk) {
                            if (SalesMargin == 0) {
                                updateInputColumn(fieldID, SalesPrice);
                            }
                            else {
                                if (SalesMargin > 0) {
                                    var Saleson = PurchasePrice * (1 + (SalesMargin / 100));
                                    updateInputColumn(fieldID, Saleson);
                                }
                                else {
                                    //updateInputColumn(fieldID, SalesPrice);
                                    if (isInclusiveChk)// for inclusive
                                    {
                                        var Inclon = InclMRP / (1 + (Math.abs(SalesMargin) / 100));
                                        updateInputColumn(fieldID, Inclon);
                                    }
                                    else { // for exclusive
                                        var Inclon = ExclMRP / (1 + (Math.abs(SalesMargin) / 100));
                                        updateInputColumn(fieldID, Inclon);
                                    }
                                }
                            }                            
                        }
                        //else {
                        //    if (SalesMargin > 0) {
                        //        var Saleson = SalesPrice / (1 + (SalesMargin / 100));
                        //        updateInputColumn(fieldID, Saleson);
                        //    }
                        //    else {
                        //        updateInputColumn(fieldID, SalesPrice);
                        //    }
                        //}
                    }
                    if (fieldName == "ECP") {
                        var ECPPrice = parseFloat($(this).attr("txttag"));
                        if (chk) {
                            if (ECPMargin == 0) {
                                updateInputColumn(fieldID, ECPPrice);
                            }
                            else
                            {
                                if (ECPMargin > 0) {
                                    var ECPon = PurchasePrice * (1 + (ECPMargin / 100));
                                    updateInputColumn(fieldID, ECPon);
                                }
                                else {
                                    if (isInclusiveChk)// for inclusive
                                    {
                                        var Inclon = InclMRP / (1 + (Math.abs(ECPMargin) / 100));
                                        updateInputColumn(fieldID, Inclon);
                                    }
                                    else { // for exclusive
                                        var Inclon = ExclMRP / (1 + (Math.abs(ECPMargin) / 100));
                                        updateInputColumn(fieldID, Inclon);
                                    }
                                }
                            }
                        }
                        //else {
                        //    if (ECPMargin > 0) {
                        //        var ECPon = ECPPrice / (1 + (ECPMargin / 100));
                        //        updateInputColumn(fieldID, ECPon);
                        //    }
                        //    else {
                        //        updateInputColumn(fieldID, ECPPrice);
                        //    }
                        //}
                    }
                    if (fieldName == "SPLPrice") {
                        var SPLPrice = parseFloat($(this).attr("txttag"));
                        if (chk) {
                            //if (SPLMargin > 0) {
                            //    var SPLon = PurchasePrice * (1 + (SPLMargin / 100));
                            //    updateInputColumn(fieldID, SPLon);
                            //}
                            //else {
                            //    updateInputColumn(fieldID, SPLPrice);
                            //}
                            if (SPLMargin == 0) {
                                updateInputColumn(fieldID, SPLPrice);
                            }
                            else {
                                if (SPLMargin > 0) {
                                    var ECPon = PurchasePrice * (1 + (SPLMargin / 100));
                                    updateInputColumn(fieldID, ECPon);
                                }
                                else {
                                    if (isInclusiveChk)// for inclusive
                                    {
                                        var Inclon = InclMRP / (1 + (Math.abs(SPLMargin) / 100));
                                        updateInputColumn(fieldID, Inclon);
                                    }
                                    else { // for exclusive
                                        var Inclon = ExclMRP / (1 + (Math.abs(SPLMargin) / 100));
                                        updateInputColumn(fieldID, Inclon);
                                    }
                                }
