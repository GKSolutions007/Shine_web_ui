    function getExistsSRBatchDetails(nProdID, nHiderowid, arBatchRowdata, TaxPern) {
        var nbranchid = $("#cmbBranch").val();
        var nPricttype = $("#cmbPriceType").val();
        var Price = nPricttype != 4 ? arBatchRowdata[2] : (parseFloat(arBatchRowdata[3]) / TaxPern);
        var MRP = arBatchRowdata[3];
        var BatchNo = arBatchRowdata[5];
        var PKD = arBatchRowdata[6];
        var Expiry = arBatchRowdata[7];
        var SREBatch = {
            AppPriceId: nPricttype,
            BranchId: nbranchid,
            ProdId: nProdID,
            BatchNumber: BatchNo,
            PkdDate: PKD,
            ExpiryDate: Expiry,
            Price: Price,
            MRP: MRP
        };

        $.ajax({
            url: Aurl + "salesreturn/getexistssrbatch",
            type: "POST",
            contentType: "application/json; charset=utf-8",
            data: JSON.stringify(SREBatch),
            dataType: "json",
            xhrFields: {
                withCredentials: true
            },
            async: false,
            success: function (response) {
                var existssrdata = eval(response);
                console.log("response", existssrdata);
                if (existssrdata) { 
                    var getprodbatchexceptthisrow = NewTempBatch.filter(a => !(a.ProdId == nProdID && a.HiddenRowIndex == nHiderowid && a.BatchType == 0));
                    if (getprodbatchexceptthisrow) {
                        console.log("Remove Prod ID : ", nProdID, "old ", getprodbatchexceptthisrow);
                    }
                    NewTempBatch = getprodbatchexceptthisrow;
                }
                $.each(existssrdata, function (i, items) {
                  
                    var nbd = {
                        "BatchType": 0,//Exists batch
                        "ProdId": nProdID,
                        "Batch": items.BatchNumber,
                        "PKD": items.PKDDate,
                        "Expiry": items.ExpiryDate,
                        "PPrice": items.PurchasePrice,
                        "SPrice": items.SalesPrice,
                        "ECP": items.ECP,
                        "MRP": items.MRPONPrice,
                        "SPLPrice": items.SPLPrice,
                        "Return": items.ReturnPrice,
                        "TaxId": items.TaxId,
                        "TaxTypeId": 1,
                        "InclusiveYesNo": 0,
                        "HiddenRowIndex": nHiderowid
                    }
                    NewTempBatch.push(nbd);
                    console.log("New ", NewTempBatch);
                });

            },
            error: function (xhr) {
                console.error(xhr.responseText);
