            $(this).find("input").each(function (index) {
                const fieldName = $(this).attr("name");
                const fieldID = $(this).attr("id");
                if (fieldName == "Excl") {
                    ExclPrice = parseFloat($(this).attr("txttag"));
                }
                if (fieldName == "Incl") {
                    InclPrice = parseFloat($(this).attr("txttag"));
                }
                if (fieldName == "Qty") {
                    Qty = parseFloat($(this).attr("txttag"));
                }
                if (fieldName == "ProdDiscPern") {
                    ProdDisc = parseFloat($(this).attr("txttag"));
                }
                if (fieldName == "ProdDiscAmt") {
                    //ProdDisc = parseFloat($(this).attr("txttag"));
                    var price = (ExclPrice * Qty);
                    ProdDiscAmt = parseFloat((price * ProdDisc) / 100);
                    //updateInputColumn(fieldID, ProdDiscAmt);
                    dPTGoods = price;
                }
            });
            dPTGoods = (ExclPrice * Qty);
            $(this).find("input").each(function (index) {
                const fieldName = $(this).attr("name");
                const fieldID = $(this).attr("id");
                if (fieldName == "TradeDiscAmt") {
                    var val = $(this).attr("txttag") != "" ? parseFloat($(this).attr("txttag")) : 0;
                    Tradediscamt = val;
                }
                if (fieldName == "ProdDiscAmt") {
                    var val = $(this).attr("txttag") != "" ? parseFloat($(this).attr("txttag")) : 0;
                    Proddiscamt = val;
                }
                if (fieldName == "AddnlDiscAmt") {
                    $(this).prop("disabled", value > 0 ? true : false);
                    var val = value != "" ? parseFloat(value) : 0;
                    var grsamt = (dPTGoods - Proddiscamt);
                    var damt = parseFloat(((val * grsamt) / 100));//.toFixed(DecVal)
                    //$(this).val(damt);
                    updateInputColumn(fieldID, damt);
                    Addnldiscamt = damt;
                }
                if (fieldName == "AddnlDiscPern") {
                    $(this).prop("disabled", value > 0 ? true : false);
                    var val = parseFloat(value);//.toFixed(DecVal)
                    updateInputColumn(fieldID, val);
                    //$(this).val(val);
                    //setTagValue(fieldID, fieldName, value);
                }
            });
            $(this).find("input").each(function (index) {
                const fieldName = $(this).attr("name");
                const fieldID = $(this).attr("id");
                if (fieldName == "ProdDiscAmt") {
                    Proddiscamt = parseFloat(document.getElementById(fieldID).getAttribute('txttag'));
                }
                if (fieldName == "TradeDiscAmt") {
                    Tradediscamt = parseFloat(document.getElementById(fieldID).getAttribute('txttag'));
                }
                if (fieldName == "AddnlDiscAmt") {
                    Addnldiscamt = parseFloat(document.getElementById(fieldID).getAttribute('txttag'));
                }
            });
            $(this).find("select").each(function (index) {
                const fieldName = $(this).attr("name");
                const fieldID = $(this).attr("id");
                if (fieldName == "UOM") {
                    var val = $('option:selected', this).attr('name');
                    CFValue = parseFloat(val);
                }
            });
            $(this).find("label").each(function (index) {
                const fieldName = $(this).attr("name");
                const fieldID = $(this).attr("id");
                if (fieldName == "Goods") {
                    var gds = parseFloat(dPTGoods);
                    //$(this).text(gds);
                    updateLabelColumn(fieldID, gds);
                    dPTGoods = gds;
                }
                if (fieldName == "Gross") {
