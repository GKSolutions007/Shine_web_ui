        const val = document.getElementById(id).getAttribute("txttag");
        $("#" + id).val(val);
        $("#" + id).select();
    });
    $(".txtsettag").blur(function (e) {
        var val = e.target.value;
        var id = e.target.id;
        updateInputColumn(id, val);
        CalculateValues();
    });
    function CalculateValues() {
        var DiscAmt = 0, grs = 0, tax = 0, net = 0, dpern = 0;
        var Goods = document.getElementById("txtValue").getAttribute("txttag");//ReturnTagValue("txtValue", "");
        if (Goods > 0)
        {
            DiscAmt = document.getElementById("txtDiscAmt").getAttribute("txttag");//ReturnTagValue("txtDiscAmt", "");
            grs = parseFloat(Goods) - parseFloat(DiscAmt);
            tax = (grs * parseFloat(taxPern)) / 100;
            net = grs + tax;
            dpern = (parseFloat(DiscAmt) / parseFloat(Goods)) * 100;
        }
        updateInputColumn("txtDiscAmt", DiscAmt); updateInputColumn("txtDiscPern", dpern);
        updateInputColumn("txtGrossAmt", grs); updateInputColumn("txtTaxAmt", tax);
        updateInputColumn("txtNetAmt", net);
    }
