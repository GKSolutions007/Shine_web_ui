            "DiscPern": document.getElementById("txtDiscPern").getAttribute("txttag"),//ReturnTagValue("txtDiscPern", ""),//$("#txtDiscPern").val(),
            "GrossAmt": document.getElementById("txtGrossAmt").getAttribute("txttag"),//ReturnTagValue("txtGrossAmt", ""),//$("#txtGrossAmt").val(),
            "TaxAmt": document.getElementById("txtTaxAmt").getAttribute("txttag"),//ReturnTagValue("txtTaxAmt", ""),//$("#txtTaxAmt").val(),
            "NetAmt": document.getElementById("txtNetAmt").getAttribute("txttag"),//ReturnTagValue("txtNetAmt", ""),//$("#txtNetAmt").val(),
            "TDSAmt": document.getElementById("txtTDS").getAttribute("txttag"),//ReturnTagValue("txtTDS", ""),//$("#txtTDS").val(),
            "TaxPern":taxPern,
            "Status": nTransMode == 1 || nTransMode == 3 ? 1 : nTransMode == 2 ? 3 : nStatus,
            "CurrentStatus": nStatus,
            "CBy": UID
        };
            $.ajax({
                url: Aurl + "prvoucher/save",
                type: 'POST',
                contentType: 'application/json; charset=utf-8',
                data: JSON.stringify(lstMaster),
                cache: false,
                processData: false,
                xhrFields: {
                    withCredentials: true
                },
                success: function (data) {
                    $("#btnSave").prop("disabled", false);
                    $.each(data, function (i, items) {
                        if (items.MsgID == 0) {
                            showSuccessSnackbar(items.Message);
                            if (nTransMode != 4) {
                                if (allowprint == 1) {
                                    PrintDocument(items.ID, PrintTransID, PrintConfigID);
                                } else if (allowprint == 3 || allowprint == 4) {
                                    PrintConfirmDialogMsg(items.ID, PrintTransID, PrintConfigID, "Do you want to print ?");
                                }
                            }
                            ClearForm();
                            //GetData(2,"");
                            GetFilterData();
                            modal.style.display = "none";
                        }
                        else if (items.Message == "AccountName") {
                            $("#dvErrorName").html("Account Name Already Exists in another master");
                        }
                        else {
                            //$("#divErrorOnCode").html(items.Message);
                            showErrorSnackbar(items.Message);
                        }
                    });
                }, error: function (data) {
                    $("#btnSave").prop("disabled", false);
                    if (data.status == "401") {
                        var msg = data.responseJSON.Message;
                        //showErrorSnackbar(msg + " Logout and Login again.");
                    }
                }
            });
    }
