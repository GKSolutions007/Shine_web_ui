    function GetData(nMode,nID) {
        $.ajax({
            url: Aurl + "prvoucher/get",
            type: 'get',
            contentType: 'json',
            data: { TransID: FID, Mode: nMode, ID: nID },
            xhrFields: {
                withCredentials: true
            },
            success: function (data) {
                if (nMode == 1) {
                    $('#cmbUDN,#cmbTax').find("option").remove();
                    $('#cmbUDN,#cmbTax').append($('<option/>', {
                        value: 0,
                        text: "-- Select --",
                    }));
                    arrPartyData = [], arrAccountData = [], arrTax = [], arrALPartyData = [], arrALAccountData = [];
                    nIndexarrParty = 0, nIndexarrAccount = 0;
                    $.each(data, function (i, items) {
                        if (items.FType == 1) {
                            var al = {
                                "label": items.Name,
                            }
                            arrALPartyData.push(al);
                            arrPartyData[nIndexarrParty] = items.Name;
                            nIndexarrParty++;
                        }
                        if (items.FType == 2) {
                            var al = {
                                "label": items.Name,
                            }
                            arrALAccountData.push(al);
                            arrAccountData[nIndexarrAccount] = items.Name;
                            nIndexarrAccount++;
                        }
                        if (items.FType == 3) {
                            $('#cmbUDN').append($('<option/>', {
                                value: items.ID,
                                text: items.Name,
                            }));
                        }
                        if (items.FType == 4) {
                            $("#lblDocID").text(items.Name);
                        }
                        if (items.FType == 5) {
                            var tlist = {
                                "ID": items.ID,
                                "Name": items.Name,
                                "Value":items.Code
                            }
                            arrTax.push(tlist);
                            $('#cmbTax').append($('<option/>', {
                                value: items.ID,
                                text: items.Name,
                            }));
                        }
                    });
                    SetAutlistforFields();
                }
                if (nMode == 2) {
                    Table.draw().clear();
                    if (data.length > 0) {
                        $.each(data, function (i, items) {
                            Table.row.add([
                                items.ID,
                                items.DocID,
                                items.DocDate,
                                items.RefNo,
                                items.PartyID,
                                items.FAID,
                                items.GoodsAmt,
                                items.TaxAmt,
                                items.NetAmt,
                                items.Balance,
                                items.Status,
                                items.Status == 'Amended' || items.Status == 'Cancelled' ? '' : '<a href="javascript:void(0) style="cursor: pointer;"><span class="glyphicon glyphicon-pencil clsEdit" style="font-size:16px" title="Edit"></span></a>',
                                items.Status == 'Amended' || items.Status == 'Cancelled' ? '' : '<a href="javascript:void(0) style="cursor: pointer;"><span class="glyphicon glyphicon-remove-circle clsCancel" style="font-size:16px" title="Cancel"></span></a>',
                                '<a href="javascript:void(0) style="cursor: pointer;"><span class="glyphicon glyphicon-eye-open clsView" style="font-size:16px" title="View"></span></a>',
                                '<a href="javascript:void(0)" class="clsVariant" style = "font-size:12px"><span class="glyphicon glyphicon-file clsVariant" style="font-size:12px" title="Variant"></span></a>',
                            ]).draw(false);
                        });
                    }
                    else {
                        showErrorSnackbar("No records found");
                    }
                }
                if (nMode == 3) {
                    $.each(data, function (i, items) {
                        if (nTransMode != 3)
                            $("#lblDocID").text(items.DocID);

                        $("#txtID").val(items.ID), $("#dtpDate").val(items.DocDate),
                            $("#txtRefNo").val(items.RefNo), $("#txtPartyName").val(items.PartyID), $("#txtAccountName").val(items.FAID),
                            $("#txtRemarks").val(items.Remark), $("#txtNarration").val(items.Narration), $("#cmbUDN").val(items.UDFId),
                            $("#txtStatus").val(items.Status), $("#cmbTax").val(items.TaxID), $("#txtSAC").val(items.SACHSN);

                        updateInputColumn("txtValue", items.NoteValue);
                        updateInputColumn("txtDiscAmt",  items.DiscAmt); updateInputColumn("txtDiscPern",  items.DiscPern);
                        updateInputColumn("txtGrossAmt", items.GrossAmt); updateInputColumn("txtTaxAmt",  items.TaxAmt);
                        updateInputColumn("txtNetAmt",items.NetAmt); updateInputColumn("txtTDS", items.TDSAmt);

                        taxPern = items.TaxPern;
                        modal.style.display = "block";
                    });
                }
            }, error: function (data) {
                if (data.status == "401") {
                    var msg = data.responseJSON.Message;
                    //showErrorSnackbar(msg + " Logout and Login again.");
                }
            }
        });
