    function SaveData() {
        $("#btnSave").prop("disabled", true);
        $(".clrdivs").html("");
        var name = $("#txtName").val();
        var code = $("#txtCode").val();
        var lstMaster = {
            "Mode": nMode,
            "ID": $("#txtID").val(),
            "Code": $("#txtCode").val(),
            "Name": $("#txtName").val(),
            "CustomerType": $("#cmbCustomerType").val(),
            "Ratings": $("#cmbRating").val(),
            "Billadd1": $("#txtBAdd1").val(),
            "Billadd2": $("#txtBAdd2").val(),
            "Billadd3": $("#txtBAdd3").val(),
            "Shipadd1": $("#txtSAdd1").val(),
            "shipadd2": $("#txtSAdd2").val(),
            "Shipadd3": $("#txtSAdd3").val(),
            "Pincode": $("#txtPincode").val(),
            "ContactPerson": $("#txtContactperson").val(),
            "Ph1": $("#txtPh1").val(),
            "Ph2": $("#txtPh2").val(),
            "Mob1": $("#txtMob1").val(),
            "Mob2": $("#txtMob2").val(),
            "Email": $("#txtEMail").val(),
            "PANNumber": $("#txtPAN").val(),
            "AadharNo": $("#txtAadhaar").val(),
            "DLNo20": $("#txtDL20").val(),
            "DLNo21": $("#txtDL21").val(),
            "FSSAINo": $("#txtFSSAI").val(),
            "StateID": $("#cmbState").val(),
            "GSTIN": $("#txtGSTIN").val(),
            "CreditTermID": $("#cmbCreditTerm").val(),
            "PaymentModeID": $("#cmbPayMode").val(),
            "TaxTypeID": $("#cmbTaxType").val(),
            "WeekCycle": $("#cmbWeekCycle").val(),
            "Active": $("#chkActive").is(":checked") ? 1 : 0,
            "UserID": UID,
        };
            $.ajax({
                url: Aurl + "vendormaster/save",
                type: 'POST',
                contentType: 'application/json; charset=utf-8',
                data: JSON.stringify(lstMaster),
                cache: false,
                processData: false,
                xhrFields: {
                    withCredentials: true
                },
                success: function (data) {
                    $("#btnSave").prop("disabled", false);
                    $.each(data, function (i, items) {
                        if (items.MsgID == 0) {
                            showSuccessSnackbar(items.Message);
                            ClearForm();
                            AllVendorData();
                            GetData(3);
                            modal.style.display = "none";
                            var depfrom = $("#txtDependFrom").val();
                            if (window.opener && !window.opener.closed) {
                                if (depfrom == "0") {// PB
                                    window.opener.GetData(1);
                                    //setTimeout(() => { 
                                        window.opener.GetData(3, code, 0);
                                    //}, 500);
                                    //window.opener.$('#txtHProds').val(name)
                                }                               
                                window.close();
                            }
                        }
                        else if (items.Message == "Code") {
                            $("#divErrorOnCode").html("Code Already Exists");
                            showErrorSnackbar("Code Already Exists");
                        }
                        else if (items.Message == "Name") {
                            $("#divErrorOnName").html("Name Already Exists");
                            showErrorSnackbar("Name Already Exists");
                        }
                        else if (items.Message == "AccountName") {
                            $("#divErrorOnName").html("Account Name Already Exists in another master");
                            showErrorSnackbar("Account Name Already Exists in another master");
                        }
                        else if (items.Message == "OSPending") {
                            //$("#divErrorOnName").html("Outstanding Amount Pending. Cannot Deactivate");
                            showErrorSnackbar("Outstanding Amount Pending. Cannot Deactivate");
                        }
                        else {
                            //$("#divErrorOnCode").html(items.Message);
                            showErrorSnackbar(items.Message);
                        }
                    });
                }, error: function (data) {
                    $("#btnSave").prop("disabled", false);
                    if (data.status == "401") {
                        var msg = data.responseJSON.Message;
                        //showErrorSnackbar(msg + " Logout and Login again.");
                    }
                }
            });
    }
