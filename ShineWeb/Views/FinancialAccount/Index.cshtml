        TransMode = 3;
        $(".clrdiv").html("");
        $("#lblPopupheading").text("View " + $("#MainHeading").text());
        rowdata = Table.row($(this).parents('tr')).data();
        //var ID = rowdata[0];
        var ID = rowdata.ID;
