    //});
    var Table = $('#tblMasterData').DataTable();
    AllFAData();
    function AllFAData() {        
        $.ajax({
            url: Aurl + "financialaccount/getalldata",
                type: "GET",
                    datatype: "JSON",
                        xhrFields: {
                withCredentials: true
            },
            data: { Mode: 3, Name: "" },
            success: function (data) {
                var fajsdata = eval(data);
                var searchValue = '';
                if ($.fn.DataTable.isDataTable('#tblMasterData')) {
                    searchValue = $('#tblMasterData').DataTable().search(); // get current search term
                }
                $('#tblMasterData').DataTable({pageLength: ItemsperPage,lengthMenu: [[10, 20, 50, 100, -1], [10, 20, 50, 100, "All"]],
                    scrollY: "450px",
                    scrollX: true,
                    scrollCollapse: true,
                    paging: true,
                    destroy: true,
                    data: fajsdata,
                    fixedColumns: false,
                    searching: true,
                    ordering: true,
                    sorting: false,
                    columns: [
                        { "data": "ID", "autoWidth": true, "visible": false, className: "dt-body-left" },
                        { "data": "AccountName", "autoWidth": true, className: "dt-body-left" },
                        { "data": "FaGroup", "autoWidth": true, className: "dt-body-left" },
                        { "data": "SACCode", "autoWidth": true, className: "dt-body-left" },
                        { "data": "PAN", "autoWidth": true, className: "dt-body-right" },
                        { "data": "OpenBalance", "autoWidth": true, className: "dt-body-right" },
                        { "data": "FAType", "autoWidth": true, className: "dt-body-left" },
                        { "data": "Active", "autoWidth": true, className: "dt-body-center" },
                        {
                            className: "dt-body-center",
                            "mRender": function (data, type, value) {
                                var _action = '<label title="' + value.CDate + '">' + value.CBy + '</label>';
                                return _action;
                            }
                        },
                        {
                            className: "dt-body-center",
                            "mRender": function (data, type, value) {
                                var _action = '<a href="javascript:void(0) style="cursor: pointer;"><span class="glyphicon glyphicon-pencil clsEdit" style="font-size:16px" title="Edit"></span></a>';
                                return _action;
                            }, "visible": $("#txtModifyID").val() == 0 ? false : true
                        },
                        {
                            className: "dt-body-center",
                            "mRender": function (data, type, value) {
                                var _action = '<a href="javascript:void(0) style="cursor: pointer;"><span class="glyphicon glyphicon-eye-open clsView" style="font-size:16px" title="View"></span></a>';
                                return _action;
                            }, "visible": $("#txtViewID").val() == 0 ? false : true
                        },
                        { "data": "OpenBalance", "autoWidth": true, "visible": false, className: "dt-body-left" },
                        { "data": "UserAccount", "autoWidth": true, "visible": false, className: "dt-body-left" },
                    ],
                    initComplete: function () {
                        // Re-apply search value after table is fully initialized
                        this.api().search(searchValue).draw();
                    }
                });
                Table = $('#tblMasterData').DataTable();
            }
        });
        
    }


    GetData(3);
    GetData(5);
    var arrData = [], arrAccGrp = [];
    function GetData(nMode,strName="") {
        $.ajax({
            url: Aurl + "financialaccount/get",
            type: 'get',
            contentType: 'json',
            data: { Mode: nMode, Name: strName },
            xhrFields: {
                withCredentials: true
            },
            //async: false,
            success: function (data) {
                if (data.length > 0) {
                    if (nMode == 3) {
                        arrData = [];
                        //Table.draw().clear();
                        $.each(data, function (i, items) {
                            //arrData[i] = items.AccountName
                            var al = {
                                "label": items.AccountName,
                                "id": items.ID,
                            }
                            arrData.push(al);
                            //Table.row.add([
                            //    items.ID,
                            //    items.AccountName,
                            //    items.FaGroup,
                            //    items.SACCode,
                            //    items.PAN,
                            //    parseFloat(items.OpenBalance).toFixed(DecVal),
                            //    items.FAType == "1" ? "Credit" : "Debit",
                            //    items.Active == "True" ? "Yes" : "No",
                            //    '<a href="javascript:void(0) style="cursor: pointer;"><span class="glyphicon glyphicon-pencil clsEdit" style="font-size:16px" title="Edit"></span></a>',
                            //    '<a href="javascript:void(0) style="cursor: pointer;"><span class="glyphicon glyphicon-eye-open clsView" style="font-size:16px" title="View"></span></a>',
                            //    items.OpenBalance,
                            //    items.UserAccount
                            //]).draw(false);
                        });

                        $("#txtAccName").autocomplete({
                            minLength: 0,
                            source: arrData,
                            select: function (event, ui) {
                                $("#txtAccName").val(ui.item.label);
                                //$("#txtRemarks").val(ui.item.id);
                                return false;
                            },
                        }).autocomplete("instance")._renderItem = function (ul, item) {
                            return $("<li class='each'>")
                                .append("<div class='acItem'><span class='name'>" +
                                    item.label + "</span></div>")
                                .appendTo(ul);
                        };
                    }
                    else if (nMode == 5) {
                        arrAccGrp = [];
                        $.each(data, function (i, items) {
                            var al = {
                                "label": items.Name,
                                "id": items.ID,
                            }
                            arrAccGrp.push(al);
                        });
                        $("#txtAccGrp").autocomplete({
                            minLength: 0,
                            source: arrAccGrp,
                            select: function (event, ui) {
                                $("#txtAccGrp").val(ui.item.label);
                                //$("#txtRemarks").val(ui.item.id);
                                return false;
                            },
                        }).autocomplete("instance")._renderItem = function (ul, item) {
                            return $("<li class='each'>")
                                .append("<div class='acItem'><span class='name'>" +
                                    item.label + "</span></div>")
                                .appendTo(ul);
                        };
                    }
                    else if (nMode == 6) {
                        $.each(eval(data), function (i, items) {
                            var tt = items.FAType == "Credit" ? "1" : "2";
                            $("#txtID").val(items.ID);
                            $("#txtAccGrp").val(items.FaGroup);
                            $("#txtAccName").val(items.AccountName);
                            $("#cmbFAType").val(tt);
                            $("#txtSAC").val(items.SACCode);
                            $("#txtPAN").val(items.PAN);
                            $("#chkActive").prop("checked", items.Active == "True");
                            updateInputColumn("txtOpenBal", items.OpenBalance);
                            $("#lblCloseOS").text("Closing Balance : " + items.CloseBal);
                            if (TransMode == 2)
                                $("#txtAccGrp").prop("disabled", items.UserAccount == "0");
                            else
                                $("#txtAccGrp").prop("disabled", true);
                        });
                    }
                }
                else {
                    showErrorSnackbar("No Records Found");
                }
            }, error: function (data) {
                if (data.status == "401") {
                    var msg = data.responseJSON.Message;
                    //showErrorSnackbar(msg + " Logout and Login again.");
                }
            }
        });
    }
    function SetTagZero() {
        var inputs = $(".txtsettag");
        for (var i = 0; i < inputs.length; i++) {
            var field = $(inputs[i]);
            var FID = field[0].id;
            if (FID != "")
                updateInputColumn(FID, 0);
        }
    }
    function updateInputColumn(inputID, netValue) {
        //const label = document.getElementById(`dgvProdGross${rowId}`);
        const input = document.getElementById(inputID);
        var value = netValue != "" ? netValue : 0;
        input.setAttribute('txttag', parseFloat(value).toFixed(4));
        input.value = parseFloat(value).toFixed(DecVal);
        input.title = netValue;
    }
    $(".txtsettag").focusin(function (e) {
        var id = e.target.id;
        const val = document.getElementById(id).getAttribute("txttag");
        $("#" + id).val(val);
        $("#" + id).select();
    });
    $(".txtsettag").blur(function (e) {
        var val = e.target.value;
        var id = e.target.id;
        updateInputColumn(id, val);
    });
    function displayResult(item) {
        $('.alert').show().html('You selected <strong>' + item.value + '</strong>: <strong>' + item.text + '</strong>');
    }
    var modal = document.getElementById("myModalfg");
    var span = document.getElementsByClassName("closefg")[0];
    span.onclick = function () {
        modal.style.display = "none";
    }
    $('#btnClose').click(function (e) {
        if (ClosecfFocus != 3) { CloseConfirmDialogMsg("Do you want to close?"); } else { CloseForm(); }
    });
    function CloseForm() {
        modal.style.display = "none";
    }
    $("#btnAdd").click(function () {
        TransMode = 1;
        $("#chkActive,#btnSave,#btnClear,.FormclrField,#cmbFAType").prop("disabled", false);
        $("#lblPopupheading").text("Add " + $("#MainHeading").text());
        modal.style.display = "block";
        ClearForm();
    });
    $('#btnClear').click(function (e) {
        if (cfFocus != 3) { ClearConfirmDialogMsg("Do you want to clear?"); } else { ClearForm(); }
    });
    function ClearForm() {
        $(".FormclrField").val("");
        $("#cmbBank").val(0)
        $("#chkDefault").prop("checked", false);
        $("#chkActive").prop("checked", true);
        $("#chkActive").prop("disabled", TransMode != 2);
        $(".clrdiv").html("");
        $("#txtAccName").focus();
        $("#lblCloseOS").text("");
        SetTagZero();
        if (TransMode != 1)
            modal.style.display = "none";
    }
    $("#btnSave").click(function () {
        $(".clrdiv").html("");
        var IsValid = true, ChkFormation = true;
        if ($("#txtAccGrp").val() == "") {
            IsValid = false;
            $("#dvErrorAccGrp").html("Account Group should not be empty");
            showErrorSnackbar("Account Group should not be empty");
        }
        else {
            const exists = arrAccGrp.filter(acc => acc.label == $("#txtAccGrp").val().trim());
            if (exists.length == 0) {
                IsValid = false;
                $("#dvErrorAccGrp").html("Account Group select from list");
                showErrorSnackbar("Account Group select from list");
            }
        }
        if ($("#txtAccName").val() == "") {
            IsValid = false;
            $("#dvErrorAccName").html("Account Name should not be empty");
            showErrorSnackbar("Account Name should not be empty");
        }
        ChkFormation = SACValidation($("#txtSAC"), $("#dvErrorSAC"));
        if (!ChkFormation) {
            IsValid = false;
        }
        ChkFormation = PANValidation($("#txtPAN"), $("#dvErrorPAN"), false);
        if (!ChkFormation) {
            IsValid = false;
        }
        if (IsValid) {
            if (cfFocus != 3) { ConfirmDialogMsg("Do you want to save data?"); } else { SaveData(); }
        }
    });
    function SaveData() {
        $("#btnSave").prop("disabled", true);
        $(".clrdiv").html("");
        var lstMaster = {
            "Mode": TransMode,
            "ID": $("#txtID").val(),
            "FaGroup": $("#txtAccGrp").val(),
            "AccountName": $("#txtAccName").val(),
            "OpenBalance": $("#txtOpenBal").val() != "" ? document.getElementById("txtOpenBal").getAttribute("txttag") : 0,
            "SACCode": $("#txtSAC").val(),
            "PAN": $("#txtPAN").val(),
            "FAType": $("#cmbFAType").val(),
            "Active": $("#chkActive").is(":checked") ? 1 : 0,
            "UserID": UID,
            "UserAccount" : 1
        };
            $.ajax({
                url: Aurl + "financialaccount/save",
                type: 'POST',
                contentType: 'application/json; charset=utf-8',
                data: JSON.stringify(lstMaster),
                cache: false,
                processData: false,
                xhrFields: {
                    withCredentials: true
                },
                success: function (data) {
                    $("#btnSave").prop("disabled", false);
                    $.each(data, function (i, items) {
                        if (items.MsgID == 0) {
                            showSuccessSnackbar(items.Message);
                            ClearForm();
                            AllFAData();
                            GetData(3);
                            modal.style.display = "none";
                        }
                        else if (items.Message == "OSPending") {
                            //$("#divErrorOnName").html("Outstanding Amount Pending. Cannot Deactivate");
                            showErrorSnackbar("Outstanding Amount Pending. Cannot Deactivate");
                        }
                        else {
                            $("#dvErrorAccName").html(items.Message);
                            showErrorSnackbar(items.Message);
                        }
                    });
                }, error: function (data) {
                    $("#btnSave").prop("disabled", false);
                    if (data.status == "401") {
                        var msg = data.responseJSON.Message;
                        //showErrorSnackbar(msg + " Logout and Login again.");
                    }
                }
            });
    }
