                                $(this).css("border-color", "red");
                                var errmsg = PMID == "7" ? "Select Credit Term" : "Select Bank Account";
                                showErrorSnackbar(errmsg);
                                IsValid = false;
                                BankAccID = 0;
                                BankAccNo = "";
                            } else {
                                BankAccID = $(this).val();
                                BankAccNo = $(this).val() > 0 ? $(this).text() : "";
                            }
                        }
                    }
                    if (fieldName == "dgvPMChqNo") {
                        if (PMID == "2") {
                            if (Amount > 0 && $(this).val() == 0) {
                                $(this).css("border-color", "red");
                                showErrorSnackbar("Select Cheque No");
                                IsValid = false;
                                TransMode = 0;
                            } else {
                                TransMode = $(this).val();
                            }
                        }
                    }
                    if (fieldName == "dgvPMTransMode")
                    {
                        if (PMID == "4") {
                            if (Amount > 0 && $(this).val() == 0) {
                                $(this).css("border-color", "red");
                                showErrorSnackbar("Select Transfer Mode");
                                IsValid = false;
                                TransMode = 0;
                            } else {
                                TransMode = $(this).val();
                            }
                        }

                    }
                });
            });
        }
        else {
            IsValid = false;
            showErrorSnackbar("Adjust Value in anyone Payment Mode");
        }
        if (IsValid) {
            if (Bal > 0) {
                IsValid = false;
                showErrorSnackbar("₹ " + Bal + " Pending, Adjust full Value");
            }
        }
        return IsValid;
    }
