    function BatchAcceptValidation() {
        var IsValid = true;
        var TrkBat = $("#txtTrackBatch").val();
        var TrkPKD = $("#txtTrackPKD").val();
        var Qty = 0;
        $("#tblBatch tbody tr").each(function () {
            $(this).find("input").each(function (index) {
                $(this).css("border-color", "black");
                const fieldName = $(this).attr("name");
                const fieldID = $(this).attr("id");
                var PKDD = "";
                if (fieldName == "BatchNo") {
                    var val = $(this).val();
                    if (TrkBat) {
                        if (val == "") {
                            IsValid = false;
                            showErrorSnackbar("Batch No Should not be empty");
                            $(this).css("border-color", "red");
                        }
                    }
                }
                if (fieldName == "PKDDate") {
                    var val = $(this).val();
                    if (TrkPKD) {
                        if (val == "") {
                            IsValid = false;
                            showErrorSnackbar("PKD Should not be empty");
                            $(this).css("border-color", "red");
                        } else {
                            PKDD = val;
                        }
                    }
                }
                if (fieldName == "ExpiryDate") {
                    var val = $(this).val();
                    if (TrkPKD) {
                        if (val == "") {
                            IsValid = false;
                            showErrorSnackbar("Expiry Should not be empty");
                            $(this).css("border-color", "red");
                        }
                        else if (val < PKDD) {
                            IsValid = false;
                            showErrorSnackbar("Expiry  Should lessthan be PKD");
                            $(this).css("border-color", "red");
                        }
                    }
                }
                else if (fieldName == "Qty") {
                    GridVal = $(this).attr("txttag");
                    Qty += parseFloat(GridVal);
                }
                else if (fieldName == "FreeQty") {
                    GridVal = $(this).attr("txttag");
                    Qty += parseFloat(GridVal);
                }
                else if (fieldName == "DmgQty") {
                    GridVal = $(this).attr("txttag");
                    Qty += parseFloat(GridVal);
                }
            });
        });
        if (BatchMode == 0) {
            if (ProductTable.rows().count() > 0) {
                $("#tblProductData tbody tr").each(function () {
                    rowdata = ProductTable.row($(this)).data();
                    var RIndex = ProductTable.row($(this)).index();
                    var ProdID = rowdata[0];
                    if (ProdID == $("#txtHProdID").val()) {
                        IsValid = false;
                        showErrorSnackbar("This Item already added in Row " + (RIndex + 1));
                        return;
                    }
                });
            }
        }
        if (Qty == 0) {
            IsValid = false;
            showErrorSnackbar("Qty should not be empty. Give anyone Qty before accept the batch");
        }
        return IsValid;
    }
