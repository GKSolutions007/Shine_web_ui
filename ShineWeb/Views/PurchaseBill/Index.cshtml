            $("#divErrProduct").html("Add atleat one product");
            showErrorSnackbar("Add atleat one product");
        }
        else {
            if (BatchAcceptArray.length == 0) {
                IsValid = false;
                $("#divErrProductBatch").html("Add Batch details for added products");
                showErrorSnackbar("Add Batch details for added products");
            }
        }
        $("#lblRoundNet").css("color", "black");
        var valu = document.getElementById("lblRoundNet").getAttribute("lbltag");
        if (parseFloat(valu) <= 0) {
            IsValid = false;
            $("#lblRoundNet").css("color", "red");
            showErrorSnackbar("Net Value should be greater than Zero");
        }
        if (IsValid)
        {
            if ($("#cmbPaymode").val() == 8 && nIsDraft == 0 && nTransMode != 4) {
                if (PaymodeArray.length == 0) {
                    IsValid = false;
                    modalPayment.style.display = "block";
                }
                else {
                    var Tot = parseFloat(document.getElementById("txtPMPTotal").getAttribute("txttag"));
                    var AdjAmt = parseFloat(document.getElementById("txtPMPAdjAmt").getAttribute("txttag"));
                    if (Tot != AdjAmt) {
                        IsValid = false;
                        var Bal = Tot - AdjAmt;
                        showErrorSnackbar("₹ " + Bal + " Pending, Adjust full Value");
                        modalPayment.style.display = "block";
                    }
                }
                if (IsValid && SaveMode == 2) {
                    IsValid = false;
                    modalPayment.style.display = "block";
                }
            }
            else {
                IsValid = true;
            }
        }
        return IsValid;
