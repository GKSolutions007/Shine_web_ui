            IsValid = false;
            $("#lblRoundNet").css("color", "red");
            //alert("Net Value should be greater than Zero");
            showErrorSnackbar("Net Amount should be greater than Zero");
        }
        if (IsValid) {
            if ($("#cmbPaymode").val() == 8 && nIsDraft == 0) {
                if (PaymodeArray.length == 0) {
                    IsValid = false;
                    modalPayment.style.display = "block";
                }
                else {
                    var Tot = parseFloat(document.getElementById("txtPMPTotal").getAttribute("txttag"));
                    var AdjAmt = parseFloat(document.getElementById("txtPMPAdjAmt").getAttribute("txttag"));
                    if (Tot != AdjAmt) {
                        IsValid = false;
                        var Bal = Tot - AdjAmt;
                        showErrorSnackbar("₹ " + Bal + " Pending, Adjust full Value");
                        modalPayment.style.display = "block";
                    }
                }
                if (IsValid && SaveMode == 2) {
                    IsValid = false;
                    modalPayment.style.display = "block";
                }
            }
            else {
                IsValid = true;
            }

        }
        if (IsValid) {
            IsValid = ValidateDuplicateitem();
        }
        if (IsValid) {
            if ($("#cmbPaymode").val() == 7 && nIsDraft == 0 && !CreditLimValidate) {
                modalCreditLimPassword.style.display = "none";
