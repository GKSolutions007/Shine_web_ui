                IsValid = false;
                $("#divErrCreditTerm").html("Select Credit Term");
                showErrorSnackbar("Select Credit Term");
            }
        }
        if ($("#dtpDate").val() == "") {
            IsValid = false;
            $("#divErrDate").html("Date should not be empty");
            showErrorSnackbar("Date should not be empty");
        }
        if ($("#lblPTNet").text() == "") {
            IsValid = false;
            //$("#divErrDate").html("Party should not be empty");
            //alert("Net Amount should be greater than Zero");
            showErrorSnackbar("Net Amount should be greater than Zero");
        }
        else {
            if (parseFloat($("#lblPTNet").text()) < 0) {
                IsValid = false;
                //$("#divErrDate").html("Party should not be empty");
                //alert("Net Amount should be greater than Zero");
                showErrorSnackbar("Net Amount should be greater than Zero");
            }
        }
        var ProdRowCounts = ProductTable.rows().count();
