                    const fieldID = $(this).attr("id");
                    if (fieldName == "dgvPMAccNo") {
                        if (PMID == "4" || PMID == "5" || PMID == "7") {
                            if (Amount > 0 && $(this).val() == 0) {
                                $(this).css("border-color", "red");
