    function ApplyRoundoff(dAmt) {
        var Value = 0;
        if (ConfigRoundtype == 1) {//nearest
            Value = Math.round(dAmt)
        }
        else if (ConfigRoundtype == 2) {//lowest
            Value = Math.floor(dAmt)
        }
        else if (ConfigRoundtype == 3) {//highest
            Value = Math.ceil(dAmt)
        }
        return Value;
    }
