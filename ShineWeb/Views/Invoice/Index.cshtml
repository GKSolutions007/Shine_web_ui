                    url: Aurl + "invoice/validatecreditlimit",
                    type: 'get',
                    contentType: 'json',
                    data: { CustID: nCustID, Date: $("#dtpDate").val(), CreditTermID: $("#cmbCreditTerm").val(), NetAmt: $("#lblRoundNet").text(), TransID: nID },
                    xhrFields: {
                        withCredentials: true
                    },
                    async: false,
                    success: function (data) {
                        if (data.length > 0) {
                            $.each(data, function (i, items) {
                                TableCreditLimPassword.row.add([
