    function ValidateDuplicateitem() {
        var isTrue = true;
        const table = $('#tblProductData').DataTable();
        const prodMap = {};
        const duplicates = [];
        var grt = 1;
        var TempProdid = 0;
        // Loop through all rows
        table.rows().every(function () {
            const data = this.data();
            const prodID = data[0];      // ProdID (hidden)
            const netAmtid = $(data[16])[2].id; // Net Amount (label)
            const netAmtStr = document.getElementById(netAmtid).getAttribute('txttag');// $(data[25]).text(); // Net Amount (label)
            const netAmt = parseFloat(netAmtStr) || 0;
            console.log("Prodmap : ", prodMap[prodID]);
            if (prodMap[prodID]) {
                console.log("Net Amt : " + netAmt);
                prodMap[prodID].count++;
                prodMap[prodID].netAmt += netAmt;
                if (parseFloat(netAmt).toFixed(2) <= 1)
                    prodMap[prodID].lesser1rs++;
                //if (parseFloat(netAmt).toFixed(2) > 1)
                else
                    prodMap[prodID].greater1rs++;

            } else {
                prodMap[prodID] = { count: 1, netAmt: netAmt, lesser1rs: 0, greater1rs: netAmt > 1 ? 1 : 0 };//greater1rs: netAmt > 1 ? grt++ : 0
            }
            TempProdid = prodID;
        });
        console.log("Duplicate Products:", prodMap);
        // Extract duplicates
        for (const prodID in prodMap) {
            if (prodMap[prodID].count > 1) {
                duplicates.push({
                    ProdID: prodID,
                    Count: prodMap[prodID].count,
                    TotalNetAmt: prodMap[prodID].netAmt.toFixed(2),
                    One_rs_less_count: prodMap[prodID].lesser1rs,
                    One_rs_greater_count: prodMap[prodID].greater1rs,
                });
            }
        }

        // Output results
        if (duplicates.length > 0) {
            console.log("🔁 Duplicate Products Found:");
            console.table(duplicates);
            //alert("Duplicate Products Found!\n" + JSON.stringify(duplicates, null, 2));
            $("#tblProductData tbody tr").each(function () {
                $(this).css("background-color", "white");
            });
            if (Invoiceallowduplicateitem == 0) {
                for (var i = 0; i < duplicates.length; i++) {
                    if (duplicates[i].One_rs_greater_count > 1) {
                        var dupProdid = duplicates[i].ProdID;
                        $("#tblProductData tbody tr").each(function () {
                            var rdata = ProductTable.row($(this)).data();
                            var rowprodid = rdata[0];
                            if (rowprodid == dupProdid) {
                                $(this).css("background-color", "lightskyblue");
                                isTrue = false;
                            }
                            //else {
                            //    $(this).css("background-color", "white");
                            //}
                        });
                    }
                }
            }

        } else {
            //alert("✅ No Duplicate Products Found.");
        }
        if (!isTrue) {
            showErrorSnackbar("Highlighted items are have multiple batches with greater than 1 Rs value");
        }
        return isTrue;
    }
