    function calculateProdGrid() {
        $(".lblPT").text(0.00);
        var dPTQty = 0, dPTFreeQty = 0, dPTDmgQty = 0, dPTGoods = 0, dPTGross = 0, dPTTax = 0, dPTNet = 0, TotProdDisc = 0, TotTradeDisc = 0, TotAddnlDisc = 0, Value = 0;
        var dQty = 0, dGross = 0, dNet = 0, DiffExcl = 0, DiffIncl = 0, nUOMCF = 1;
        var nvcount = ProductTable.rows().count();

        if (nvcount > 0) {
            $("#tblProductData tbody tr").each(function () {
                var allValues = {};
                var rowdata = ProductTable.row($(this)).data();
                var TInvoicePrice = parseFloat(rowdata[28]);
                var TaxPern = parseFloat(rowdata[2]);
                $(this).find("input").each(function (index) {
                    const fieldName = $(this).attr("name");
                    const fieldID = $(this).attr("id");

                    if (fieldName == "Qty") {
                        const fullValue = document.getElementById(fieldID).getAttribute('txttag');
                        Value = fullValue != "" ? fullValue : 0;
                        //Value = $(this).val() != "" ? $(this).val() : 0;
                        dPTQty += parseFloat(Value);
                        dQty = parseFloat(Value);
                    }
                    if (fieldName == "ProdDiscAmt") {
                        const fullValue = document.getElementById(fieldID).getAttribute('txttag');
                        Value = fullValue != "" ? fullValue : 0;
                        //Value = $(this).val() != "" ? $(this).val() : 0;
                        TotProdDisc += parseFloat(Value);
                    }
                    if (fieldName == "TradeDiscAmt") {
                        const fullValue = document.getElementById(fieldID).getAttribute('txttag');
                        Value = fullValue != "" ? fullValue : 0;
                        //Value = $(this).val() != "" ? $(this).val() : 0;
                        TotTradeDisc += parseFloat(Value);
                    }
                    if (fieldName == "AddnlDiscAmt") {
                        const fullValue = document.getElementById(fieldID).getAttribute('txttag');
                        Value = fullValue != "" ? fullValue : 0;
                        //Value = $(this).val() != "" ? $(this).val() : 0;
                        TotAddnlDisc += parseFloat(Value);
                    }
                    //allValues[fieldName] = $(this).val();
                });
                $(this).find("select").each(function (index) {
                    const fieldName = $(this).attr("name");
                    const fieldID = $(this).attr("id");
                    if (fieldName == "UOM") {
                        nUOMCF = $("#" + fieldID + " option:selected").attr('name');
                    }
                });
                $(this).find("label").each(function (index) {
                    const fieldName = $(this).attr("name");
                    const fieldID = $(this).attr("id");
                    if (fieldName == "Goods") {
                        const fullValue = document.getElementById(fieldID).getAttribute('lbltag');
                        Value = fullValue != "" ? fullValue : 0;
                        dPTGoods += parseFloat(Value);

                    }
                    if (fieldName == "Gross") {
                        const fullValue = document.getElementById(fieldID).getAttribute('lbltag');
                        Value = fullValue != "" ? fullValue : 0;
                        dPTGross += parseFloat(Value);
                        dGross = parseFloat(Value);
                    }
                    if (fieldName == "TaxAmt") {
                        const fullValue = document.getElementById(fieldID).getAttribute('lbltag');
                        Value = fullValue != "" ? fullValue : 0;
                        dPTTax += parseFloat(Value);
                    }
                    if (fieldName == "NetAmt") {
                        const fullValue = document.getElementById(fieldID).getAttribute('lbltag');
                        Value = fullValue != "" ? fullValue : 0;
                        dPTNet += parseFloat(Value);
                        dNet = parseFloat(Value);
                    }
                    if (fieldName == "DiffValue") {
                        if (dQty > 0) {
                            var tp = parseFloat(1 + (TaxPern / 100));
                            var val = dGross / dQty;
                            var Diff = val - (TInvoicePrice * nUOMCF);
                            var AbsDiff = Diff;// Math.abs(Diff);

                            DiffExcl += (AbsDiff * dQty);

                            //val = dNet / dQty;
                            //Diff = val - TInvoicePrice;
                            //AbsDiff = Diff;// Math.abs(Diff);
                            DiffIncl += ((AbsDiff * dQty) * tp);

                        } else {
                            updateLabelColumn(fieldID, 0);
                            $(this).css("color", "black");
                            DiffExcl += 0;
                            DiffIncl += 0;
                        }
                    }
                    //allValues[fieldName] = $(this).val();
                });
                if (dNet == 0) {
                    $(this).css("background-color", "lightgrey");
                }
                else {
                    $(this).css("background-color", "white");
                }
            });
        }
        updateLabelColumn("lblPTQty", dPTQty);
        updateLabelColumn("lblPTPDiscAmt", TotProdDisc); updateLabelColumn("lblPTTDiscAmt", TotTradeDisc); updateLabelColumn("lblPTADiscAmt", TotAddnlDisc);
        //$("#lblPTQty").text(dPTQty.toFixed(DecVal)); $("#lblPTFreeQty").text(dPTFreeQty.toFixed(DecVal)); $("#lblPTDmgQty").text(dPTDmgQty.toFixed(DecVal));
        //$("#lblPTPDiscAmt").text(TotProdDisc.toFixed(DecVal)); $("#lblPTTDiscAmt").text(TotTradeDisc.toFixed(DecVal)); $("#lblPTADiscAmt").text(TotAddnlDisc.toFixed(DecVal));
        updateInputColumn("txtProdDiscAmt", TotProdDisc); updateInputColumn("txtTradeDiscAmt", TotTradeDisc); updateInputColumn("txtAddnlDiscAmt", TotAddnlDisc);
